	if not LoopStaminaEnabled then
		invalid("Infinite Stamina Is Already Disabled")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Infinite Stamina Is Already Disabled"),"red")
		return
	end
	LoopStaminaEnabled = false 
	success("Disabled Infinite Stamina.")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled Infinite Stamina"),"Blue")
end   
},

["fog"] = {
Callback = function(Mod,Value)
	Value = tonumber(Value)
	if noValue(Value) then return end
	Value = math.floor(Value + .5)
	Lighting.FogEnd = tonumber(Value)
	success(string.format("Set Your Fog Level To (%s)",tostring(Value)))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set Your Fog Level To (%s)",tostring(Value)),"Blue")
end 
},

["nofog"] = {
Callback = function()
	Lighting.FogEnd = 9e9
	success("Removed Fog.")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Removed Fog"),"Blue")
end 
},

["killzom"] = {
Callback = function(Mod)
if not game.Workspace:FindFirstChild("Zombies") then
return
end
local Amount = 0
for i, v in pairs(game.Workspace.Zombies:GetDescendants()) do
if v:IsA("Humanoid") and v.Health > 0 then
	Amount = Amount + 1
	Workspace.Remote.DamageZombie:FireServer(v, 10000)
end
end
if Amount == 0 then
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: All Zombies Are Removed"),"Red")
error(string.format("All Zombies Are Removed"))
return
end
success(string.format("Removed (%s) Zombies", Amount))
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Removed (%s) Zombies",Amount),"Blue")
end
},

["spec"] = {
Callback = function(Mod,Player)
	if Player == nil then
		Player = GetPlayer(Player)
		if noPlayer(Player) then return end
	end
	if Player:lower() == "me" then
		Player = Players.LocalPlayer.Name
	end  
	Player = GetPlayer(Player)
	if noPlayer(Player) then return end
	if deadPlayer(Player) then return end
  if Workspace.CurrentCamera.CameraSubject == Players[Player].Character.Humanoid then
	invalid(string.format("Already Spectating (%s)",Player))
	 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Already Spectating (%s)",Player),"Red")
	 return
  end
	Workspace.CurrentCamera.CameraSubject = Players[Player].Character.Humanoid
	success(string.format("Spectating (%s)",Player))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Spectating (%s)",Player),"Blue")
end   
},

["offlist"] = {
Callback = function()
if offList == false then
offList = true
fireServer("ChangeParentLocal", LPlr)
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Removed You From Playerlist"),"Blue")
success("Removed You From Playerlist")
else
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: Your Already Off The Playerlist"),"Red")
end
end
},

["onlist"] = {
Callback = function()
if offList == true then
offList = false
fireServer("ChangeParentLocal", LPlr, game.Players)
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Added You Back To Playerlist"),"Blue")
success("Added You Back To Playerlist")
else
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: Your Already On The Playerlist"),"Red")
end
end
},

["serverlock"] = {
Callback = function()
if ServerLock == true then
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: ServerLock Is Already Enabled"),"Red")
invalid("ServerLock Is Already Enabled")
end
if ServerLock == false then
ServerLock = true
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: ServerLock : Enabled"),"Blue")
success("ServerLock : Enabled")
end
end

},

["unserverlock"] = {
Callback = function()
if ServerLock == false then
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: ServerLock Is Already Disabled"),"Red")
invalid("ServerLock Is Already Disabled")
else
ServerLock = false
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: ServerLock : Disabled"),"Blue")
success("ServerLock : Disabled")
end
end

},


["fullbright"] = {
Callback = function()
if FullBrightOn == true then
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: FullBright Is Already Enabled"),"Red")
invalid("FullBright is already enabled")
end
if FullBrightOn == false then
FullBrightOn = true
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: FullBright : Enabled"),"Blue")
success("FullBright : Enabled")
end
end

},


["unfullbright"] = {
Callback = function()
if FullBrightOn == false then
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: FullBright Is Already Disabled"),"Red")
invalid("FullBright is already disabled")
end
if FullBrightOn == true then
FullBrightOn = false
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: FullBright : Disabled"),"Blue")
success("FullBright : Disabled")
end
end

},
-- Players
["kill"] = {
Callback = function(Mod,Player,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		local Reason = tostring(Value)
		if notLoaded(Player.Name) then return end
		 if deadPlayer(Player.Name) then return end
		fireServer("ChangeParent", Player.Character:FindFirstChild("Head"), nil)   
		 if not Value then Reason = "None" end
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Killed (%s) | Reason : (%s)",Player.Name,Reason),"Blue")
		 success(string.format("Killed (%s) | Reason : (%s)",Player.Name,Reason))
		 --game.Workspace.Remote.Chat:FireServer("Global", "[Neptunium]: Killed ("..Player.Name .. ") | Reason : ("..Reason..")")
	end   
end
},

["loopkill"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		spawn(function()
			while true do
		if loopkillt then
		fireServer("ChangeParent", Player.Character:FindFirstChild("Head"), nil)   
			end
			wait(18)
		end
	end)
	loopkillt = true
	success(string.format("LoopKilling (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: LoopKilling (%s)",Player.Name),"Blue") 
	end  
end
},

["unloopkill"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
			loopkillt = false
		success(string.format("UnLoopKilling (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: UnLoopKilling (%s)",Player.Name),"Blue")
	end   
end
},

--offlistkill
["offlistkill"] = {
Callback = function()
for i, v in pairs(game.Workspace:GetDescendants()) do
if v.Name == "Famine" and v.Parent:FindFirstChild("Animate") and not game.Players:FindFirstChild(v.Parent.Name) then
fireServer("ChangeParent", v.Parent.Head)
success(string.format("Killed Offlist Player (%s)",v.Parent.Name))
 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Killed Offlist Player (%s)",v.Parent.Name),"Blue")
 return
else 
wait(0.1)
invalid(string.format("No Offlist Players Were Found"))
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: No Offlist Players Were Found"),"Red")
return
end
end
end
},

["rocket"] = {
Callback = function(Mod,Player)
local Playerlist = returnPlayers(Mod,Player)
for _, Player in pairs(Playerlist) do 
 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Turned (%s) Into A Rocket", Player.Name),"Blue")
success(string.format("Turned (%s) Into A Rocket",Player.Name))
local Flare = PlaceFlare(Player)
local StartPos = Flare.Head.Position
local Max = 70
if Dist ~= nil and Dist > 70 then
	Max = Dist
end
local NSpeed = 3
if Max > 70 then
	local S = Max/70-1
	NSpeed = NSpeed + S
end
local Broken = false
local Ev;
Ev = game.Workspace.ChildAdded:connect(function(Ex)
	if Ex.Name == "Explosion" and (Ex.Position-Flare.Head.Position).Magnitude < 10 then
		Broken = true
		Ev:Disconnect()
	end
end)
for i = 1, 999999 do
	if i == Max-50 then
		RM.Detonate:FireServer({["Head"] = Flare.Head})
	end
	if Broken == true or i > Max then
		break
	end
	RM.ReplicateModel:FireServer(Flare, CFrame.new(0, i*NSpeed, 0)+StartPos)
	wait()
end
fireServer("ChangeParent",v, Flare)
end
end
},

["explode"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		workspace.Remote.Detonate:FireServer({Name = "", Head = Player.Character.Head})    
		success(string.format("Exploded (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Exploded (%s)",Player.Name),"Blue")
	end   
end
