	end   
end
},

["untornado"] = {
Callback = function(Mod,Player)
	if Player == nil then 
		Workspace.Remote.SwitchEnabled:FireServer(false, ReplicatedStorage.Effects.Bullet:FindFirstChild("Sheets"))
		success("Disabled Tornado Bullets")
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled Tornado Bullets"),"Blue")
		return
	end   
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do  
		if notLoaded(Player.Name) then return end
		Workspace.Remote.SwitchEnabled:FireServer(false, Player.Character.Torso:FindFirstChild("Sheets"))
		success(string.format("Disabled Tornado Effect For (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled Tornado Effect For (%s)",Player.Name),"Blue")
	end   
end
},

["bullet"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do  
		if notLoaded(Player.Name) then return end
		if not Player.Character.Torso:FindFirstChild("Fire") then
			clone(ReplicatedStorage.Effects.Bullet:FindFirstChild("Fire"), Player.Character.Torso, 1)
		end
		Workspace.Remote.SwitchEnabled:FireServer(true, Player.Character.Torso:FindFirstChild("Fire"))
		success(string.format("Enabled Bullet Effect For (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Enabled Bullet Effect For (%s)",Player.Name),"Blue")
	end   
end
},

["unbullet"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do  
		if notLoaded(Player.Name) then return end
		Workspace.Remote.SwitchEnabled:FireServer(false, Player.Character.Torso:FindFirstChild("Fire"))
		success(string.format("Disabled Bullet Effect For (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled Bullet Effect For (%s)",Player.Name),"Blue")
	end   
end
},


-- Appreance 

["color"] = {
Callback = function(Mod,Player,Color)
	local Playerlist = returnPlayers(Mod,Player) 
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		if not Color or tonumber(Color) == nil then
			invalid("Missing Or Invaild Color Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Color Input"),"Red")
			return
		end   
		Color = math.clamp(Color, 1, 65)
		Color = math.floor(Color + .5)
		local PlayerObject = Player 
		if PlayerObject.Character then
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", PlayerObject.Character, "","","")
			PlayerObject.Character:WaitForChild("SecondaryColor")
			workspace.Remote.ColorGun:FireServer(PlayerObject.Character, Color, 1, Color, 1)
			wait(.1)
			fireServer("ChangeParent", PlayerObject.Character:WaitForChild("SecondaryColor"), nil)
		end
		success(string.format("Colored (%s) | Color Id (%s)",Player.Name,Color))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Colored (%s) | Color Id (%s)",Player.Name,Color),"Blue")
	end   
end
},

["shirt"] = {
Callback = function(Mod,Player,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		Value = tonumber(Value)
		if noValue(Value) then return end
		if not Player.Character:FindFirstChild("GameShirt") then 
			invalid(string.format("%s's Shirt Is Missing",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Shirt Is Missing"),"Red")
			return
		end
		changeShirt(Player.Name, Value)
		success(string.format("Set (%s's) Shirt ID To (%s)",Player.Name,tostring(Value)))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Shirt ID To (%s)",Player.Name,tostring(Value)),"Blue")
	end    
end
},

