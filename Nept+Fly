["exit"] = {
Callback = function()
 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Closed Console : : Bye!"),"Blue")
 fireServer("ChangeParent", Client.playerstats:FindFirstChild("meow"), nil)
 success("Bye Bye")
	 _G.silentaim = false
	 ServerLock = false
	RainbowGunEnabled = false
	RainbowPlayerEnabled = false
	RainbowBagEnabled = false
	RainbowVestEnabled = false
	Wallbang = false
	ChatHookEvent:Disconnect()
	Neptunium:Destroy()
end    
}

}

ChatHookEvent =
Workspace.Remote.Chat.OnClientEvent:connect(function(...)
local Args = {...}
local Player = Args[1]["Speaker"]
local Message = Args[1]["Message"]
if checkpermission(Player.Name) or Player == Client then
if string.sub(Message, 1, #Prefix) == Prefix then
	local formatted = string.sub(Message, 1 + #Prefix)
	fireCommand(formatted,Player.Name)
end
end
end)

function fireCommand(text, Player)
local Args = GetArgs(text)
if not Args or #Args == 0 then return end
local Command = Args[1]

if not CommandDictionary[Command] then
invalid("Invalid Command")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Invalid Command"),"Red")
Type.Text = "" 
return    
end   

if nonmods[Command] and table.find(mods, Player) then
invalid("Thats A Client Commands Only")
return
end

Player = Players[Player] == Client and 'You' or Player 
player(Player .. ": " .. "\"" .. text .. "\"")
Player = (Player == "You" and Client.Name or Player)
CommandDictionary[Command].Callback(Player,select(2, table.unpack(Args)))
Type.Text = "" 
end

Type.FocusLost:Connect(function(enterPressed)
if enterPressed and Type.Text ~= nil and Type.Text ~= "" then
fireCommand(Type.Text, Client.Name)
end    
end)

--//Drag Function 
local IsDragging2, DragInput2, DragStart2, StartPos2
Drag.InputBegan:Connect(function(input)
if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
IsDragging2 = true
DragStart2 = input.Position
StartPos2 = Console.Position

input.Changed:Connect(function()
	if input.UserInputState == Enum.UserInputState.End then IsDragging2 = false end
end)
end
end)
Drag.InputChanged:Connect(function(input)
if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then DragInput2 = input end
end)
UserInputService.InputChanged:Connect(function(input)
if input == DragInput2 and IsDragging2 then 
local Delta = input.Position - DragStart2
Console.Position = UDim2.new(StartPos2.X.Scale, StartPos2.X.Offset + Delta.X, StartPos2.Y.Scale, StartPos2.Y.Offset + Delta.Y)
end
end)
-- toggle gui
local UIS = game:GetService('UserInputService') --Gets the UserInputService
local bool = false
UIS.InputBegan:Connect(function(input) --connects a function to the input
if input.KeyCode == Enum.KeyCode.LeftAlt or input.KeyCode == Enum.KeyCode.RightAlt then-- Detects if the player pressed Q. The keycode doesnt have to be Q it can be whatever you want as long as its on your keyboard 
if bool == false then
game:GetService("CoreGui").Neptunium.Enabled = false
bool = true
else
game:GetService("CoreGui").Neptunium.Enabled = true
bool = false
end
end
end)
-- Fly
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()

local flying = false
local flySpeed = 120  -- Default speed; update from flyspeed.Text if available
local bodyVelocity = nil
local bodyGyro = nil
local controlConnection = nil
local flyConnection = nil

-- Update speed from TextBox if it exists (from GUI)
local function updateFlySpeed()
    if flyspeed and flyspeed:IsA("TextBox") then
        local newSpeed = tonumber(flyspeed.Text) or 50
        flySpeed = math.max(1, newSpeed)  -- Clamp to positive
    end
end

-- Disable game-specific anti-cheats (Apocalypse Rising)
local function disableAntiCheats()
    local pgui = LocalPlayer:FindFirstChild("PlayerGui")
    if pgui then
        local hitGui = pgui:FindFirstChild("HitEqualsYouDie")
        if hitGui then
            pcall(function()
                hitGui.WalkspeedEdit.Disabled = true
                hitGui.JumpLimiter.Disabled = true
            end)
        end
        pcall(function() pgui.SkyboxRenderMode.Disabled = true end)
    end
end

local function startFly()
    if flying then return end
    flying = true
    updateFlySpeed()
    
    local character = LocalPlayer.Character
    if not character then return end
    
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return end
    
    disableAntiCheats()
    
    -- Create BodyMovers
    bodyGyro = Instance.new("BodyGyro")
    bodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
    bodyGyro.P = 9e4
    bodyGyro.Parent = humanoidRootPart
    
    bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    bodyVelocity.Velocity = Vector3.new(0, 0.1, 0)
    bodyVelocity.Parent = humanoidRootPart
    
    local humanoid = character:FindFirstChild("Humanoid")
    if humanoid then humanoid.PlatformStand = true end
    
    -- Input controls (WASD)
    local keys = {W = 0, A = 0, S = 0, D = 0}
    
    controlConnection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        local key = input.KeyCode
        if key == Enum.KeyCode.W then keys.W = 1
        elseif key == Enum.KeyCode.S then keys.S = -1
        elseif key == Enum.KeyCode.A then keys.A = -1
        elseif key == Enum.KeyCode.D then keys.D = 1
        end
    end)
    
    UserInputService.InputEnded:Connect(function(input)
        local key = input.KeyCode
        if key == Enum.KeyCode.W then keys.W = 0
        elseif key == Enum.KeyCode.S then keys.S = 0
        elseif key == Enum.KeyCode.A then keys.A = 0
        elseif key == Enum.KeyCode.D then keys.D = 0
        end
    end)
    
    -- Fly loop (smoother with Heartbeat)
    flyConnection = RunService.Heartbeat:Connect(function()
        if not flying or not bodyVelocity or not bodyGyro then return end
        
        local camera = workspace.CurrentCamera
        local moveVector = camera.CFrame.LookVector * (keys.W + keys.S) + camera.CFrame.RightVector * (keys.D + keys.A)
        bodyVelocity.Velocity = moveVector * flySpeed
        
        bodyGyro.CFrame = camera.CFrame
    end)
end

local function stopFly()
    if not flying then return end
    flying = false
    
    if controlConnection then
        controlConnection:Disconnect()
        controlConnection = nil
    end
    if flyConnection then
        flyConnection:Disconnect()
        flyConnection = nil
    end
    
    local character = LocalPlayer.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then humanoid.PlatformStand = false end
    end
    
    if bodyVelocity then
        bodyVelocity:Destroy()
        bodyVelocity = nil
    end
    if bodyGyro then
        bodyGyro:Destroy()
        bodyGyro = nil
    end
end

-- Auto-restart on respawn
LocalPlayer.CharacterAdded:Connect(function()
    if flying then
        wait(1)  -- Wait for full load
        startFly()
    end
end)

-- GUI Toggle Hooks (adapt to your buttons)
-- FlyToggle2.MouseButton1Down:Connect(startFly)  -- Enable button
-- FlyToggle.MouseButton1Down:Connect(stopFly)    -- Disable button

-- Optional: Keybind toggle (e.g., F)
UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.F then
        if flying then stopFly() else startFly() end
    end
end)
