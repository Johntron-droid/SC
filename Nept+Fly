end  
},

["vexplode"] = {
Callback = function(Mod,Vehicle)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Engine, 0)
	success(string.format("Exploded (%s)",Vehicle))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Vehicle Exploded (%s)",Vehicle),"Blue")
end  
},

["vunweld"] = {
Callback = function(Vehicle)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do
		if v.Name == "Weld" then 
			fireServer("ChangeParent", v, nil)
		end
	end   
	success(string.format("Unanchored (%s)",Vehicle))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Vehicle Unanchored (%s)",Vehicle),"Blue")
end  
},

["vclear"] = {
Callback = function(Mod,Vehicle)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	for _, v in pairs(Workspace.Vehicles:GetDescendants()) do
		if v.Name == Vehicle then 
			fireServer("ChangeParent", v, nil)
		end
	end
	success(string.format("Cleared All (%s's)",Vehicle))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Cleared All (%s's)",Vehicle),"Blue")
end  
},

["vdelete"] = {
Callback = function(Mod,Vehicle)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	fireServer("ChangeParent", Workspace.Vehicles:FindFirstChild(Vehicle))
	success(string.format("Deleted (%s)",Vehicle))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Deleted (%s)",Vehicle),"Blue")
end  
},

["vclean"] = {
Callback = function()
if not game.Workspace:FindFirstChild("Vehicles") then
return
end
local VAmount = 0
for i, v in pairs(game.Workspace.Vehicles:GetDescendants()) do
if v.Name == "MaxSpeed" and v.Value > 100 or v.Name == "Hull" and v.Value > 5000 then
	fireServer("ChangeParent", v.Parent.Parent)
	_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Deleted (%s) | Hacked Vehicle",v.Parent.Parent.Name),"Blue")
	success(string.format("Deleted (%s) | Hacked Vehicle",v.Parent.Parent.Name))
	VAmount = VAmount + 1
	end
	end
	if VAmount <= 0 then
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: There Are No Hacked Vehicles"),"Red")
	invalid("There Are No Hacked Vehicles")
	end
end 
},

["vreset"] = {
Callback = function(Mod,Vehicle)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	fireServer("ChangeValue",Workspace.Vehicles[Vehicle].Stats.MaxSpeed,RemovedVehicles[Vehicle])
	fireServer("ChangeValue",Workspace.Vehicles[Vehicle].Stats.MaxSpeed.Offroad,RemovedVehicles[Vehicle])
	success(string.format("Reset (%s's) speed",Vehicle))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Reset (%s) speed",Vehicle),"Blue")
end  
},

["vspeed"] = {
Callback = function(Mod,Vehicle,Value)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	Value = tonumber(Value)
	if noValue(Value) then return end
	Value = math.floor(Value + .5)
	fireServer("ChangeValue",Workspace.Vehicles[Vehicle].Stats.MaxSpeed,Value)
	fireServer("ChangeValue",Workspace.Vehicles[Vehicle].Stats.MaxSpeed.Offroad,Value)
	success(string.format("Set (%s) speed to (%s)",Vehicle,tostring(Value)))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) speed to (%s)",Vehicle,tostring(Value)),"Blue")
end  
},

["vclone"] = {
Callback = function(Mod,Vehicle,Player)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end

	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notSpawned(Player.Name) then return end

		local X, Y = math.random(10, 15), math.random(10, 15)
		if math.random(1, 4) >= 3 then
			X = X*-1
		end
		if math.random(1, 4) >= 3 then
			Y = Y*-1
		end
		Clone(Player, Vehicle, Workspace.Vehicles, Vector3.new(X, 0, Y))
		success(string.format("Cloned (%s) to (%s)",Vehicle,Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Cloned (%s) to (%s)",Vehicle,Player.Name),"Blue")
	end
end
},

["vtp"] = {
Callback = function(Mod,Player,Vehicle)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do
		Vehicle = GetVehiclesName(Vehicle) 
		if noVehicle(Vehicle) then return end
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		if noHumanoid(Player.Name) then return end
		if deadPlayer(Player.Name) then return end
		Workspace.Remote.AddClothing:FireServer("driven", Player.Character, "","","")
		Workspace.Remote.AddClothing:FireServer("IsBuildingMaterial", Player.Character.HumanoidRootPart, "","","")
		Workspace.Remote.AddClothing:FireServer("SeatPoint", Player.Character.Torso, "","","")
		wait(.3)
		workspace.Remote.HurtZombie:FireServer(Player.Character)
		workspace.Remote.ReplicatePart:FireServer(Player.Character.HumanoidRootPart, Workspace.Vehicles[Vehicle].Essentials.Base.CFrame + Vector3.new(0, 10, 0))
		wait(1.1)
		fireServer("ChangeParent", Player.Character.HumanoidRootPart.IsBuildingMaterial, nil)
		fireServer("ChangeParent", Player.Character.Torso.SeatPoint, nil)
		Player.Character:WaitForChild("driven")
		fireServer("ChangeParent", Player.Character.driven, nil)
		success(string.format("Teleported (%s) to (%s)",Player.Name,Vehicle))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Teleported (%s) to (%s)",Player.Name,Vehicle),"Blue")
	end
end
},

["vbring"] = {
Callback = function(Mod,Vehicle,Player) 
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	if Player == nil then
		if noPlayer(Player) then return end
	end
	if Player:lower() == "me" then
		Player = Players.LocalPlayer.Name
	end 
	Player = GetPlayer(Player)
	if noPlayer(Player) then return end
	if notLoaded(Player) then return end
	if notSpawned(Player) then return end
	Workspace.Remote.AddClothing:FireServer("IsBuildingMaterial", Workspace.Vehicles[Vehicle].Essentials.Base, "","","")
	Workspace.Vehicles[Vehicle].Essentials.Base:WaitForChild("IsBuildingMaterial")
	Workspace.Remote.ReplicatePart:FireServer(Workspace.Vehicles[Vehicle].Essentials.Base,Players[Player].Character.HumanoidRootPart.CFrame + Vector3.new(10 ,4,0))
	wait(0.1)
	fireServer("ChangeParent",Workspace.Vehicles[Vehicle].Essentials.Base:FindFirstChild("IsBuildingMaterial"), nil)
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Brought (%s) to (%s)",Vehicle,Player),"Blue")
	success(string.format("Brought (%s) to (%s)",Vehicle,Player))
end
},

["vrainbow"] = {
	Callback = function(Mod,Vehicle)
		Vehicle = GetVehiclesName(Vehicle)
		if noVehicle(Vehicle) then return end   
		if Workspace.Vehicles[Vehicle].Stats.Armor.Value > 0 then
			for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do     
				if  v.Name == "Special" or v.Name == "Color" or v.Name == "Black" or v.Name == "White" or v.Name == "Details" or v.Name == "RoofLights" or v.Name == "DoorHandles" or v.Name == "FrontLights" or v.Name == "Grill" or v.Name == "Interior" or v.Name == "Frame" or v.Name == "LMWheel" or v.Name == "LBWheel" or v.Name == "Black" or v.Name == "LFWheel" or v.Name == "RFWheel" or v.Name == "RMWheel" or v.Name == "RBWheel" then
					Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
					v:WaitForChild("SecondaryColor")
					spawn(function()
						while true do 
						if RainbowCarEnabled then
							local Random = math.random(16, 20)
							Workspace.Remote.ColorGun:fireServer(v, Random, 1, Random, 1)
							fireServer("VehichleLightsSet",v, "Neon", 0)
							end
							wait(0.1)
							end 
					end)
				end
			end
		else
			for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do     
				if  v.Name == "Special" or v.Name == "Color" or v.Name == "Black" or v.Name == "White" or v.Name == "Details" or v.Name == "RoofLights" or v.Name == "DoorHandles" or v.Name == "FrontLights" or v.Name == "Grill" or v.Name == "Interior" or v.Name == "Frame" or v.Name == "LMWheel" or v.Name == "LBWheel" or v.Name == "Black" or v.Name == "LFWheel" or v.Name == "RFWheel" or v.Name == "RMWheel" or v.Name == "RBWheel" then
					Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
					v:WaitForChild("SecondaryColor")
					spawn(function()
						while true do 
						if RainbowCarEnabled then
							local Random = math.random(16, 20)
							Workspace.Remote.ColorGun:fireServer(v, Random, 1, Random, 1)
							fireServer("VehichleLightsSet",v, "Neon", 0)
							end
							wait(0.1)
							end 
					end)
				end
			end
		end
		if Vehicle == "PoliceCar" then
			success(string.format("Enabled Vehicle (%s) Rainbow Effect",Vehicle))
			_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Enabled Vehicle (%s) Rainbow Effect",Vehicle),"Blue")
			return
		end  
		for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do     
			if  v.Name == "ArmorSkirt" then
				Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
				v:WaitForChild("SecondaryColor")
				spawn(function()
					while true do 
					if RainbowCarEnabled then
						local Random = math.random(16, 20)
						Workspace.Remote.ColorGun:fireServer(v, Random, 1, Random, 1)
						fireServer("VehichleLightsSet",v, "Neon", 0)
						end
						wait(0.1)
					end 
				end)
			end
		end
		RainbowCarEnabled = true
		success(string.format("Enabled Vehicle (%s) Rainbow Effect",Vehicle))
		_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Enabled Vehicle (%s) Rainbow Effect",Vehicle),"Blue")
	end  
	},

["vunrainbow"] = {
		Callback = function(Mod,Vehicle)  
		Vehicle = GetVehiclesName(Vehicle)
		if noVehicle(Vehicle) then return end
		RainbowCarEnabled = false
		for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do
			if  v.Name == "SecondaryColor" then
				fireServer("ChangeParent",v, nil)
			end
		end
		success(string.format("Disabled Vehicle (%s) Rainbow Effect",Vehicle))
		_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled Vehicle (%s) Rainbow Effect",Vehicle),"Blue")
		 end
		},

-- Server

["cleanloot"] = {
Callback = function(Mod)
	local VAmount = 0
	for _, v in pairs(Workspace:GetChildren()) do
		if Lighting.LootDrops:FindFirstChild(v.Name) then
			fireServer("ChangeParent",v, nil)
			VAmount = VAmount + 1
		end
	end      
	wait()
	if VAmount > 0 then
	success(string.format("Cleaned (%s) Items Of Spawned Loot",VAmount))
	_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Cleaned (%s) Items Of Spawned Loot",VAmount),"Blue") 
	return
	end
	wait()
	if VAmount == 0 then
		_G_env["_G"].SendMessage(string.format("[!][Neptunium]: No Spawned Items Were Found"),"Red")
		invalid("No Spawned Items Were Found")
	return 
   end
end  
},

["cleanclones"] = {
Callback = function(Mod)
	local VAmount = 0
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 or game.PlaceId == 290815963 or game.PlaceId == 1228677761 then
		for _, v in pairs(Workspace:GetChildren()) do
			if Workspace["Anchored Objects"]["Towns/Cities"]:FindFirstChild(v.Name) then
				fireServer("ChangeParent", v, nil)
				VAmount = VAmount + 1
			end
		end  
	end
	for _, v in pairs(Workspace:GetChildren()) do
		if v.Name == "HeliCrash" and v:FindFirstChild("IsBuildingMaterial") or v.Name == "SpawnPlate" then 
			fireServer("ChangeParent", v, nil)
			VAmount = VAmount + 1
		end
	end
	if VAmount > 0 then
		success(string.format("Cleaned (%s) Location/Heli Clones",VAmount))
		_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Cleaned (%s) Location/Heli Clones", VAmount),"Blue")
		return
		end
	if VAmount == 0 then
		invalid("No Location/Heli Clones Were Found")
		_G_env["_G"].SendMessage(string.format("[!][Neptunium]: No Location/Heli Clones Were Found"),"Red")
	end
end
},

