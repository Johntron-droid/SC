["clog"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if noClient(Player.Name) then return end
		fireServer("ChangeValue", Player.playerstats:FindFirstChild("combat"),true)
		wait(0.25)
		fireServer("ChangeParent",Player,nil) 
		wait()
		success(string.format("CombatLogged (%s).",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: CombatLogged (%s)",Player.Name),"Blue")
	end   
end
},

["heal"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		if noHumanoid(Player.Name) then return end
		if deadPlayer(Player.Name) then return end
		fireServer("Damage", Player.Character.Humanoid, -100)
		success(string.format("Healed (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Healed (%s)",Player.Name),"Blue")
	end   
end
},

["god"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if noHumanoid(Player.Name) then return end
		if deadPlayer(Player.Name) then return end
		if Player.Character.Humanoid:FindFirstChild("DefenseMultiplier") then
				_G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Is Already Godded",Player.Name),"Red")
				invalid(string.format("(%s) Is Already Godded",Player.Name))
		end    			
		wait() 
		if not Player.Character.Humanoid:FindFirstChild("DefenseMultiplier") then
		Workspace.Remote.AddClothing:FireServer("DefenseMultiplier",Player.Character.Humanoid,0,"","")
		success(string.format("Godded (%s)",Player.Name))
		_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Godded (%s)",Player.Name),"Blue")
		end
	end
end
},

["ungod"] = {
Callback = function(Mod,Player)  
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if noHumanoid(Player.Name) then return end
		if deadPlayer(Player.Name) then return end
		if not Player.Character.Humanoid:FindFirstChild("DefenseMultiplier") then
			_G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Is Not Godded",Player.Name),"Red")
			invalid(string.format("(%s) Is Not Godded",Player.Name))
		end
		wait()
		if Player.Character.Humanoid:FindFirstChild("DefenseMultiplier") then
			fireServer("ChangeParent", Player.Character.Humanoid.DefenseMultiplier, nil)
			success(string.format("UnGoded (%s)",Player.Name))
			_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: UnGodded (%s)",Player.Name),"Blue")
	end    
	end
end
},   

["ban"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do
		if noClient(Player.Name) then return end
		local PlayerObject = Player
		banData[tostring(PlayerObject.UserId)] = true
		writefile("Neptunium_Bans.txt",HttpService:JSONEncode(banData))
		KickPlayer(PlayerObject , .25)
		success(string.format("Banned (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Banned (%s)",Player.Name),"Blue")
	end   
end
},

["addban"] = {
Callback = function(Mod,Player)
	if noClient(Player) then return end
	local id
	pcall(function ()
		id = Players:GetUserIdFromNameAsync(Player)
	end)
	if id then
		id = tostring(id)
		banData[id] = true
		writefile("Neptunium_Bans.txt",HttpService:JSONEncode(banData))
	else
		invalid("Invalid Player")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Invalid Player"),"Red")
		return
	end
	if Players:FindFirstChild(Player) then
		PlayerObject = Players[Player]
		KickPlayer(PlayerObject, .25)
	end    
	success(string.format("Banned (%s)",Player))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Added (%s) To Banlist",Player),"Blue")
end   
},

["unban"] = {
Callback = function(Mod,Player)
	local id
	pcall(function ()
		id = Players:GetUserIdFromNameAsync(Player)
	end)
	if id then
		id = tostring(id)
		banData[id] = nil
		writefile("Neptunium_Bans.txt",HttpService:JSONEncode(banData))
	else
		invalid("Invalid Player")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Invalid Player"),"Red")
		return
	end
		success(string.format("Unbanned (%s) ",tostring(Player)))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Unbanned (%s) ",tostring(Player)),"Blue")
	return         
end 
},

["dropinv"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notSpawned(Player.Name) then return end   
		for _, v in pairs (Player.playerstats.slots:GetChildren()) do
			if v:IsA("IntValue") then
				 fireServer("ChangeValue", v, 2)
			end
		end
		wait()
		success(string.format("Made (%s) Items Drop.",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Made (%s) Items Drop",Player.Name),"Blue")
	end   
end
},

["admin"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if noClient(Player.Name) then return end
		if table.find(mods, Player.Name) then 
			invalid(string.format("(%s) Already Has Admin Abilitys",Player.Name))
			_G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Already Has Admin Abilitys",Player.Name),"Red")
			return
		end
		table.insert(mods, Player.Name)
		success(string.format("Gave (%s) Admin Abilitys",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Gave (%s) Admin Abilitys",Player.Name),"Blue")
	end
end
},

["unadmin"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if noClient(Player.Name) then return end
		if table.find(mods, Player.Name) then 
			table.remove(mods, table.find(mods, Player.Name))
			success(string.format("Removed (%s) Admin Abilitys",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Removed (%s) Admin Abilitys",Player.Name),"Blue")
			else 
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Does Not Have Admin Abilitys",Player.Name),"Red")
invalid(string.format("(%s) Does Not Have Admin Abilitys",Player.Name))
		end
	end
end
},

["tp"] = {
Callback = function(Mod,Player,Player2)
	local Playerlist = returnPlayers(Mod,Player)
	local Player2 = returnPlayers(Mod,Player2)[1]
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		if noHumanoid(Player.Name) then return end
		if deadPlayer(Player.Name) then return end
		if Player2 == nil then return end
		if notLoaded(Player2.Name) then return end
		if notSpawned(Player2.Name) then return end
		if noHumanoid(Player2.Name) then return end
		if deadPlayer(Player2.Name) then return end
		if Player == Player2 then 
			invalid(string.format("(%s) Is There Own Player",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Is There Own Player",Player.Name),"Red")
			return
		end
		Workspace.Remote.AddClothing:FireServer("driven", Player.Character, "","","")
		Workspace.Remote.AddClothing:FireServer("IsBuildingMaterial", Player.Character.Head, "","","")
		Workspace.Remote.AddClothing:FireServer("SeatPoint", Player.Character.Torso, "","","")
		wait(.3)
		Workspace.Remote.HurtZombie:FireServer(Player.Character)
		Workspace.Remote.ReplicatePart:FireServer(Player.Character.Head, Player2.Character.Torso.CFrame + Vector3.new(-3, 0, 0)) 
		wait(1.1)
		fireServer("ChangeParent", Player.Character.Head.IsBuildingMaterial, nil)
		fireServer("ChangeParent", Player.Character.Torso.SeatPoint, nil)
		Player.Character:WaitForChild("driven")
		fireServer("ChangeParent", Player.Character.driven, nil)
		success(string.format("Teleported (%s) To (%s)", Player.Name,Player2.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Teleported (%s) To (%s)", Player.Name,Player2.Name),"Blue")
	end
end
},

["loc"] = {
Callback = function(Mod,Player,Location)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Location = GetLocation(Location)
		if noLocation(Location) then return end

		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		if noHumanoid(Player.Name) then return end
		if deadPlayer(Player.Name) then return end

		if not Workspace.Locations[Location]:IsA("BasePart") then 
			invalid("Invalid Location")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Invalid Location"),"Red")
			return 
		end

		Workspace.Remote.AddClothing:FireServer("driven", Player.Character, "","","")
		Workspace.Remote.AddClothing:FireServer("IsBuildingMaterial", Player.Character.Head, "","","")
		Workspace.Remote.AddClothing:FireServer("SeatPoint", Player.Character.Torso, "","","")
		wait(.1)
		Workspace.Remote.HurtZombie:FireServer(Player.Character)
		Workspace.Remote.ReplicatePart:FireServer(Player.Character.Head, Workspace.Locations[Location].CFrame + Vector3.new(0, 6, 0))
		wait(1.1)
		fireServer("ChangeParent", Player.Character.Head.IsBuildingMaterial, nil)
		fireServer("ChangeParent", Player.Character.Torso.SeatPoint, nil)
		Player.Character:WaitForChild("driven")
		fireServer("ChangeParent", Player.Character.driven, nil)

		success(string.format("Teleported (%s) To (%s)",Player.Name,Location))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Teleported (%s) To (%s)",Player.Name,Location),"Blue")
	end
end
},

["bring"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 

		if Loaded() then return end
		if Spawned() then return end
		if Humanoid() then return end
		if Dead() then return end

		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		if noHumanoid(Player.Name) then return end
		if deadPlayer(Player.Name) then return end

		if Player == Client then 
			invalid(string.format("(%s) Is There Own Player",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Is There Own Player",Player.Name),"Red")
			return
		end
		Workspace.Remote.AddClothing:FireServer("driven", Player.Character, "","","")
		Workspace.Remote.AddClothing:FireServer("IsBuildingMaterial", Player.Character.Head, "","","")
		Workspace.Remote.AddClothing:FireServer("SeatPoint", Player.Character.Torso, "","","")
		wait(.3)
		Workspace.Remote.HurtZombie:FireServer(Player.Character)
		Workspace.Remote.ReplicatePart:FireServer(Player.Character.Head, Players.LocalPlayer.Character.Torso.CFrame + Vector3.new(-3, 0, 0))
		wait(1.1)
		fireServer("ChangeParent", Player.Character.Head.IsBuildingMaterial, nil)
		fireServer("ChangeParent", Player.Character.Torso.SeatPoint, nil)
		Player.Character:WaitForChild("driven")
		fireServer("ChangeParent", Player.Character.driven, nil)

		success(string.format("Brought (%s) To You.",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Brought (%s) To You.",Player.Name),"Blue")
	end
end
},

["hunger"] = {
Callback = function(Mod,Player,Value) 
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Value = tonumber(Value)
		if noValue(Value) then return end
		fireServer("ChangeValue", Player.playerstats.Hunger, Value)
		success(string.format("Set (%s) Hunger To (%s)",Player.Name, tostring(Value)))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Hunger To (%s)",Player.Name, tostring(Value)),"Blue")
	end
end
},

