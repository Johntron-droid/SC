["resetmap"] = {
Callback = function(Mod)
	if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 then
	success("Reseting map to orginal state. please wait...  (Try Not To Move)")
		  _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Reseting map to orginal state. please wait...  (Try not to move much)"),"Blue") 
		for _, v in pairs(Workspace["TerrainChunks"]:GetChildren()) do
			fireServer("VehichleLightsSet",v, "SmoothPlastic", 0)
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 8, 10, 8, 10)                 	
			end
	end
	if not Workspace["Anchored Objects"] then 
		invalid("Map Is Deleted")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map Is Deleted"),"Red")
		return
	end
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
	success("Reseting map to orginal state. please wait...")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Reseting map to orginal state. please wait..."),"Blue")
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", Workspace["Anchored Objects"].Plates, "","","")
		Workspace["Anchored Objects"].Plates:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(Workspace["Anchored Objects"].Plates, 8, 10, 8,10)
		fireServer("ChangeParent", Workspace["Anchored Objects"].Plates:FindFirstChild("SecondaryColor"), nil)
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills:GetChildren()) do
			task.spawn(function()
		  fireServer("VehichleLightsSet",v, "SmoothPlastic",  0)
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 8, 10, 8,10)
			end)
		end
			for _, v in pairs(Workspace["Anchored Objects"]:GetChildren()) do
			if v.Name == "MapWalls" then
		  fireServer("VehichleLightsSet",v, "Plastic",  1)
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 8, 10, 8,10)
			end
		end
		wait()
			for _, v in pairs(Workspace["Anchored Objects"].Plates:GetChildren()) do
			if v.Name == "Roads" then
			fireServer("VehichleLightsSet",v, "SmoothPlastic",  0)
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 36, 12, 36,12)
			end
		end
			for _, v in pairs(Workspace["Anchored Objects"].Plates:GetChildren()) do
			if v.Name == "Dirt Roads" then
			fireServer("VehichleLightsSet",v, "SmoothPlastic",  0)
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 33, 1, 33,1)
			end
		end				
			for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"].FortRuins.Spec_Ruins1E:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			fireServer("VehichleLightsSet",v, "SmoothPlastic",  0)
			Workspace.Remote.ColorGun:FireServer(v, 36, 12, 36,12)
		 end
		 for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"].FortRuins.Spec_Ruins1E:GetChildren()) do
			if v.Name == "Grey" then
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			fireServer("VehichleLightsSet",v, "SmoothPlastic",  0)
			Workspace.Remote.ColorGun:FireServer(v, 64, 1, 64,1)
		 end
		 end
			for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"].Factory.Spec_Factory1E:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			fireServer("VehichleLightsSet",v, "SmoothPlastic",  0)
			Workspace.Remote.ColorGun:FireServer(v, 36, 12, 36,12)
		 end
				  for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"].Factory.Spec_Factory1E.FactoryBuilding:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			fireServer("VehichleLightsSet",v, "SmoothPlastic",  0)
			Workspace.Remote.ColorGun:FireServer(v, 36, 12, 36,12)
		 end
			for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"].RadioStation.Spec_RadioTower1E:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			fireServer("VehichleLightsSet",v, "SmoothPlastic",  0)
			Workspace.Remote.ColorGun:FireServer(v, 1, 12, 1,12)
		 end
		 for _, v in pairs(Workspace["Anchored Objects"].Plates.Fields:GetDescendants()) do
			task.spawn(function()
		 Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			fireServer("VehichleLightsSet",v, "SmoothPlastic",  0)
			Workspace.Remote.ColorGun:FireServer(v, 47, 1, 47,1)
			end)
		end
			for _, v in pairs(Workspace["Anchored Objects"].Plates:GetChildren()) do
			if v.Name == "Roads" or v.Name == "Dirt Roads" or v.Name == "Fields" or v.Name == "Hills" then		
				fireServer("VehichleLightsSet",Workspace["Anchored Objects"].Plates, "Grass", 0)
			end
		end	
		for _, v in pairs(Workspace["Anchored Objects"]:GetDescendants()) do
			if v.Name == "SecondaryColor" then
				fireServer("ChangeParent", v, nil)
			end
		end
		wait(.5)
		
	end   
	if game.PlaceId == 290815963 or game.PlaceId == 1228677761 then --//Amend
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", Workspace["Anchored Objects"].Plates.Plates, "","","")
		Workspace["Anchored Objects"].Plates.Plates:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(Workspace["Anchored Objects"].Plates.Plates,8, 10, 8, 10) 
		fireServer("ChangeParent", Workspace["Anchored Objects"].Plates.Plates:FindFirstChild("SecondaryColor"), nil)
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills.MediumHills:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 8, 10, 8, 10)
		end
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills.LargeHills:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 8, 10, 8, 10)
		end
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills.SmallHills:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v ,8, 10, 8, 10)
		end
		wait(.1)
		for _, v in pairs(Workspace["Anchored Objects"]:GetDescendants()) do
			if v.Name == "SecondaryColor" then
				fireServer("ChangeParent", v, nil)
			end
		end
	end
	success("Successfully Reset " .. Workspace.mapname.Value .. "'s Map Colors.")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Successfully Reset " .. Workspace.mapname.Value .. "'s Map Colors"),"Blue")
end
},

["box"] = {
Callback = function(Mod,Color,Texture)
	if not Color or tonumber(Color) == nil then
		invalid("Missing Or Invaild Color Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Color Input"),"Red")
		return
	end   
	if not Texture or tonumber(Texture) == nil then
		invalid("Missing Or Invaild Texture Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Texture Input"),"Red")
		return
	end   
	Color = math.clamp(Color, 1, 65)
	Color = math.floor(Color + .5)
	Texture = math.clamp(Texture, 1, 12)
	Texture = math.floor(Texture + .5)
	if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 or game.PlaceId == 290815963 or game.PlaceId == 1228677761 then 
		invalid("Coloring Is Disabled On This Map")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Coloring Is Disabled On This Map"),"Red")
		return
	end
	if not workspace["Anchored Objects"] then 
		invalid("Map Is Deleted")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map Is Deleted"),"Red")
		return
	end
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", Workspace["Anchored Objects"], "","","")
		Workspace["Anchored Objects"]:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(Workspace["Anchored Objects"]["MapWalls"], Texture, Color,Texture)
		fireServer("ChangeParent", Workspace["Anchored Objects"]:FindFirstChild("SecondaryColor"), nil)
		wait()
		for _, v in pairs(Workspace["Anchored Objects"]:GetChildren()) do
		if v.Name == "MapWalls" then
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v,Color, Texture, Color,Texture)
			end
		end
		wait()
		for _, v in pairs(Workspace["Anchored Objects"]:GetDescendants()) do
			if v.Name == "SecondaryColor" then
				fireServer("ChangeParent", v, nil)
			end
		end
	end   
	success("Colored/Textured " .. Workspace.mapname.Value.. "'s Map Walls")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Colored/Textured " .. Workspace.mapname.Value.. "'s Map Walls | Color Id (%s) | Texture Id (%s)",Color,Texture),"Blue")
end
},

["boxmat"] = {
Callback = function(Mod,Material,Value)
	Material = GetMaterials(Material)
	if not Material then
		invalid("Missing Or Invalid Material Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Material Input"),"Red")
		return
	end   
	Value = tonumber(Value)
	if noValue(Value) then return end
	Value = math.clamp(Value, 0, 1)
	if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 or game.PlaceId == 290815963 or game.PlaceId == 1228677761 then 
		invalid("Changing Material Is Disabled On This Map")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Changing Material Is Disabled On This Map"),"Red")
		return
	end
	if not workspace["Anchored Objects"] then 
		invalid("Map Is Deleted")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map Is Deleted"),"Red")
		return
	end
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
		for _, v in pairs(Workspace["Anchored Objects"]["MapWalls"]:GetChildren()) do
			fireServer("VehichleLightsSet",v, Material, Value)
		end
		for _, v in pairs(Workspace["Anchored Objects"]:GetChildren()) do
			if v.Name == "MapWalls" then
				fireServer("VehichleLightsSet",Workspace["Anchored Objects"]["MapWalls"], Material, Value)
			end
		end
	end   
	success(string.format("Set " .. Workspace.mapname.Value.. "'s walls Material to (%s) and transparency (%s)",Material,Value))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set " .. Workspace.mapname.Value.. "'s Map Walls Material to (%s) and transparency (%s)",Material,Value),"Blue")
end
},


--Building parts

["pcolor"] = {
Callback = function(Mod,Part,Color,Texture)
	Part = GetParts(Part)  
	if noPart(Part) then return end

	if not Color or tonumber(Color) == nil then
		invalid("Missing Or Invaild Color Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Color Input"),"Red")
		return
	end   
	if not Texture or tonumber(Texture) == nil then
		invalid("Missing Or Invaild Texture Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Texture Input"),"Red")
		return
	end    
	Color = tonumber(Color)
	Color = math.clamp(Color, 1, 65)
	Color = math.floor(Color + .5) 
	Texture = tonumber(Texture)
	Texture = math.clamp(Texture, 1, 12)
	Texture = math.floor(Texture + .5) 

	local PartObject = Lighting.Materials[Part] 
	Workspace.Remote.AddClothing:FireServer("SecondaryColor", PartObject, "","","")
	PartObject:WaitForChild("SecondaryColor")
	workspace.Remote.ColorGun:FireServer(PartObject, Color, Texture, Color, Texture)
	wait(.1)
	fireServer("ChangeParent", PartObject:WaitForChild("SecondaryColor"), nil)
	success(string.format("Colored/Textured Material (%s)",Part))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Colored/Textured Material (%s) | Color Id (%s) | Texture Id (%s)",Part,Color,Texture),"Blue")
end  
},

["preset"] = {
Callback = function(Mod,Part) 
	Part = GetParts(Part)  
	if noPart(Part) then return end
	local PartObject = Lighting.Materials[Part] 

	Workspace.Remote.AddClothing:FireServer("SecondaryColor", PartObject, "", "", "")
	PartObject:WaitForChild("SecondaryColor")    
	workspace.Remote.ColorGun:FireServer(PartObject,table.unpack(Resets[Part]))
	wait(.1)
	fireServer("ChangeParent", PartObject:WaitForChild("SecondaryColor"), nil)

	if Part == "LargeCrateOpen" then
		local path = Lighting.Materials.LargeCrateOpen
		fireServer("VehichleLightsSet", path,"Plastic", 0)
	end

	if Part == "SmallCrateOpen" then
		local path = Lighting.Materials.SmallCrateOpen
		fireServer("VehichleLightsSet", path,"Plastic", 0)
	end
	success(string.format("Changed (%s) Back To Normal Color.",Part))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Changed (%s) Back To Normal Color.",Part),"Blue")
end
},

["pmat"] = {
Callback = function(Mod,Part,Material,Value) 
	Part = GetParts(Part)  
	if noPart(Part) then return end
	Material = GetMaterials (Material)
	if not Material then
		invalid("Missing Or Invalid Material Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Material Input"),"Red")
		return
	end   
	if noValue(Value) then return end
	Value = tonumber(Value)
	Value = math.clamp(Value, 0, 1)
	if Part == "Floodlight" then 
		invalid("Floodlight's Material Can Not Be Changed")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Floodlight's Material Can Not Be Changed"),"Red")
		return 
	end
	fireServer("VehichleLightsSet",Lighting.Materials[Part],Material,Value)
	success(string.format("Success, Set (%s's) Material to (%s)",Part,Material))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Material to (%s)",Part,Material),"Blue")
end
},

-- Particles

["blood"] = {
Callback = function(Mod,Player)
	if Player == nil then 
		if not ReplicatedStorage.Effects.Bullet:FindFirstChild("Blood") then
			Workspace.Remote.SwitchEnabled:FireServer(false,Lighting:FindFirstChild("FireEffect"))
			clone(ReplicatedStorage.Effects:FindFirstChild("Blood"), ReplicatedStorage.Effects.Bullet, 1)
		end
		Workspace.Remote.SwitchEnabled:FireServer(true, ReplicatedStorage.Effects.Bullet:FindFirstChild("Blood"))
		ReplicatedStorage.Effects:WaitForChild("Blood")
		Workspace.Remote.SwitchEnabled:FireServer(true,ReplicatedStorage.Effects:FindFirstChild("Blood"))
		success("Enabled Blood Bullets.")
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Enabled Blood Bullets"),"Blue")
		return
	end

	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do  
		if notLoaded(Player.Name) then return end
		if not Player.Character.Torso:FindFirstChild("Blood") then
			clone(ReplicatedStorage.Effects:FindFirstChild("Blood"), Player.Character.Torso, 1)
		end
		Workspace.Remote.SwitchEnabled:FireServer(true, Player.Character.Torso:FindFirstChild("Blood"))
		success(string.format("Enabled Blood Effect For (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Enabled Blood Effect For (%s)",Player.Name),"Blue")
	end   
end
},

["unblood"] = {
Callback = function(Mod,Player)
	if Player == nil then 
		Workspace.Remote.SwitchEnabled:FireServer(false, ReplicatedStorage.Effects.Bullet:FindFirstChild("Blood"))
		success("Disabled Blood Bullets")
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled Blood Bullets"),"Blue")
		return
	end   

	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do  
		if notLoaded(Player.Name) then return end
		Workspace.Remote.SwitchEnabled:FireServer(false, Player.Character.Torso:FindFirstChild("Blood"))
		success(string.format("Disabled Blood Effect For (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled Blood Effect For (%s)",Player.Name),"Blue")
