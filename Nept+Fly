["factmat"] = {
Callback = function(Mod,Material,Value)
	Material = GetMaterials(Material)
	if not Material then
		invalid("Missing Or Invalid Material Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Material Input"),"Red")
		return
	end   
	Value = tonumber(Value)
	if noValue(Value) then return end
	Value = math.clamp(Value, 0, 1)
	if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 or game.PlaceId == 290815963 or game.PlaceId == 1228677761 then 
		invalid("Changing Material Is Disabled On This Map")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Changing Material Is Disabled On This Map"),"Red")
		return
	end
	if not workspace["Anchored Objects"] then 
		invalid("Map Is Deleted")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map Is Deleted"),"Red")
		return
	end
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
		for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"].Factory.Spec_Factory1E:GetChildren()) do
			fireServer("VehichleLightsSet",v, Material, Value)
			end
	   
		for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"].Factory.Spec_Factory1E.FactoryBuilding:GetChildren()) do
			fireServer("VehichleLightsSet",v, Material, Value)
			end
wait()
			for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"]["Factory"]:GetDescendants()) do
			fireServer("VehichleLightsSet",v, Material, Value)
			end
wait()
for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"]["Factory"]:GetDescendants()) do
if v.Name == "Ind_Container1" or v.Name == "Ind_Container2" or  v.Name == "Ind_Container3" or v.Name == "Ind_Container4" or v.Name == "Ind_Container5" then
fireServer("ChangeParent",v, Workspace["ARCL_WALLS"])
fireServer("VehichleLightsSet", Workspace["ARCL_WALLS"], Material, Value)
			end
			end	
			wait(2)
for _, v in pairs(Workspace["ARCL_WALLS"]:GetChildren()) do
if v.Name == "Ind_Container1" or v.Name == "Ind_Container2" or  v.Name == "Ind_Container3" or v.Name == "Ind_Container4" or v.Name == "Ind_Container5" then
fireServer("ChangeParent",v, Workspace["Anchored Objects"]["Towns/Cities"]["Factory"])
	end
	end

end
	success(string.format("Set " .. Workspace.mapname.Value.. "'s Roads Material to (%s) and transparency (%s)",Material,Value))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set " .. Workspace.mapname.Value.. "'s Factory Material to (%s) and transparency (%s)",Material,Value),"Blue")
end
},

["kinmat"] = {
Callback = function(Mod,Material,Value)
	Material = GetMaterials(Material)
	if not Material then
		invalid("Missing Or Invalid Material Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Material Input"),"Red")
		return
	end   
	Value = tonumber(Value)
	if noValue(Value) then return end
	Value = math.clamp(Value, 0, 1)
	if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 or game.PlaceId == 290815963 or game.PlaceId == 1228677761 then 
		invalid("Changing Material Is Disabled On This Map")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Changing Material Is Disabled On This Map"),"Red")
		return
	end
	if not workspace["Anchored Objects"] then 
		invalid("Map Is Deleted")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map Is Deleted"),"Red")
		return
	end
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
		for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"]["Kin"]:GetDescendants()) do
			fireServer("VehichleLightsSet",v, Material, Value)
			end			
	for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"]["Kin"]:GetDescendants()) do
if v.Name == "Ind_Container1" or v.Name == "Ind_Container2" or  v.Name == "Ind_Container3" or v.Name == "Ind_Container4" or v.Name == "Ind_Container5" then
fireServer("ChangeParent",v, Workspace["ARCL_WALLS"])
fireServer("VehichleLightsSet", Workspace["ARCL_WALLS"], Material, Value)
			end
			end	
			wait(4)
for _, v in pairs(Workspace["ARCL_WALLS"]:GetChildren()) do
if v.Name == "Ind_Container1" or v.Name == "Ind_Container2" or  v.Name == "Ind_Container3" or v.Name == "Ind_Container4" or v.Name == "Ind_Container5" then
fireServer("ChangeParent",v, Workspace["Anchored Objects"]["Towns/Cities"]["Kin"])
fireServer("VehichleLightsSet",Workspace["Anchored Objects"]["Towns/Cities"]["Kin"].Lights, "Plastic", 1)
		  end
	 end
end
	success(string.format("Set " .. Workspace.mapname.Value.. "'s Kin Material to (%s) and transparency (%s)",Material,Value))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set " .. Workspace.mapname.Value.. "'s Kin City Material to (%s) and transparency (%s)",Material,Value),"Blue")
end
},


