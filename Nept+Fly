["rainbow"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player) 
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
	local PlayerObject = Player 
			Workspace.Remote.AddClothing:fireServer("SecondaryColor", PlayerObject.Character, "","","")
			PlayerObject.Character:WaitForChild("SecondaryColor")
spawn(function()
	while true do 
	if RainbowPlayerEnabled then
			local Random = math.random(16, 20)
			Workspace.Remote.ColorGun:fireServer(PlayerObject.Character, Random, 1, Random, 1)
		  end
		  wait(0.1)
	end
end)
		RainbowPlayerEnabled = true
	Event = Player.Character.Humanoid.Died:Connect(function()
		RainbowPlayerEnabled = false
		Event:Disconnect()
		end)
		success(string.format("Enabled (%s) Rainbow Player",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Player To Rainbow",Player.Name),"Blue")
	end   
end
},


["unrainbow"] = {
Callback = function(Mod,Player)  
local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
	if notLoaded(Player.Name) then return end
	if notSpawned(Player.Name) then return end	
RainbowPlayerEnabled = false
success(string.format("Disabled (%s) Rainbow Player",Player.Name))
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled (%s) Rainbow Player",Player.Name),"Blue")
 end
end
},

--Weapon skins 

["skin"] = {
Callback = function(Mod,Player,Color,Texture,Color2,Texture2)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if not Color or tonumber(Color) == nil then
			invalid("Missing Or Invaild Color Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Color Input"),"Red")
			return
		end   
		if not Texture or tonumber(Texture) == nil then
			invalid("Missing Or Invaild Texture Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Texture Input"),"Red")
			return
		end   
		if not Color2 or tonumber(Color2) == nil then
			invalid("Missing Or Invaild Color Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Color Input"),"Red")
			return
		end   
		if not Texture2 or tonumber(Texture2) == nil then
			invalid("Missing Or Invaild Texture Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Texture Input"),"Red")
			return
		end   
		Color = tonumber(Color)
		Color = math.clamp(Color, 1, 65)
		Color = math.floor(Color + .5)
		Texture = tonumber(Texture)
		Texture = math.clamp(Texture, 1, 12)
		Texture =  math.floor(Texture + .5) 
		Color2 = tonumber(Color2)
		Color2 = math.clamp(Color2, 1, 65)
		Color2 = math.floor(Color2 + .5)
		Texture2 = tonumber(Texture2)
		Texture2 = math.clamp(Texture2, 1, 12)
		Texture2 =  math.floor(Texture2 + .5) 
		local skinSlot
		for i = 1, 100 do
			local skin = Player.playerstats.skins["skin" .. tostring(i)]
			if skin.Value == 0 then
				skinSlot = skin
				break
			end
		end
		if skinSlot == nil then
			invalid(string.format("(%s) Skin Slots Are Full",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s's) Skin Slots Are Full",Player.Name),"Red")
			return
		end 
		fireServer("ChangeValue",skinSlot, Color)
		fireServer("ChangeValue",skinSlot.material, Texture)
		fireServer("ChangeValue",skinSlot.secondary, Color2)
		fireServer("ChangeValue",skinSlot.secondary.material,Texture2)
		success(string.format("Gave Custom Skin To (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Gave Custom Skin To (%s) | Top: (%s) (%s) | Bottom: (%s) (%s)",Player.Name,Color,Texture,Color2,Texture2),"Blue")
	end 
end
},

["noskins"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		for i, v in pairs(Player.playerstats.skins:GetChildren()) do
			fireServer("ChangeValue", v.Parent["skin" .. tostring(i)], 0)
			fireServer("ChangeValue", v.Parent["skin" .. tostring(i)].material, 0)
			fireServer("ChangeValue", v.Parent["skin" .. tostring(i)].secondary, 0)
			fireServer("ChangeValue", v.Parent["skin" .. tostring(i)].secondary.material, 0)
		end
		success(string.format("Removed (%s) Weapon Skins",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Removed (%s) Weapon Skins",Player.Name),"Blue")
	end 
	
end
},

["skinkit"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		spawn(function()
			for i,v in pairs(Player.playerstats.skins:GetChildren()) do 
				fireServer("ChangeValue", v.Parent["skin" .. tostring(1)], 22)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(1)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(1)].secondary, 21)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(1)].secondary.material, 1)
			wait()
				fireServer("ChangeValue", v.Parent["skin" .. tostring(2)], 21)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(2)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(2)].secondary, 21)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(2)].secondary.material, 1)
			wait()
				fireServer("ChangeValue", v.Parent["skin" .. tostring(3)], 20)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(3)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(3)].secondary, 20)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(3)].secondary.material, 1)
			wait()
				fireServer("ChangeValue", v.Parent["skin" .. tostring(4)], 19)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(4)].material, 9)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(4)].secondary, 22)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(4)].secondary.material, 2)
			wait()
				fireServer("ChangeValue", v.Parent["skin" .. tostring(5)], 20)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(5)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(5)].secondary, 22)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(5)].secondary.material, 1)
			wait()
				fireServer("ChangeValue", v.Parent["skin" .. tostring(6)], 18)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(6)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(6)].secondary, 22)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(6)].secondary.material, 1)
			wait()
				fireServer("ChangeValue", v.Parent["skin" .. tostring(7)], 16)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(7)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(7)].secondary, 16)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(7)].secondary.material, 1)
			wait()
				fireServer("ChangeValue", v.Parent["skin" .. tostring(8)], 27)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(8)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(8)].secondary, 27)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(8)].secondary.material, 1)
			wait()
				fireServer("ChangeValue", v.Parent["skin" .. tostring(9)], 17)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(9)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(9)].secondary, 22)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(9)].secondary.material, 1)
			wait()
				fireServer("ChangeValue", v.Parent["skin" .. tostring(10)], 64)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(10)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(10)].secondary, 64)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(10)].secondary.material, 1)
			wait() 
				fireServer("ChangeValue", v.Parent["skin" .. tostring(11)], 24)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(11)].material, 9)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(11)].secondary, 24)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(11)].secondary.material, 9)
			wait()   
				fireServer("ChangeValue", v.Parent["skin" .. tostring(12)], 17)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(12)].material, 9)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(12)].secondary, 17)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(12)].secondary.material, 9)			
			wait()   
				fireServer("ChangeValue", v.Parent["skin" .. tostring(13)], 18)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(13)].material, 9)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(13)].secondary, 18)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(13)].secondary.material, 9)
			wait()   
				fireServer("ChangeValue", v.Parent["skin" .. tostring(14)], 29)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(14)].material, 1)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(14)].secondary, 21)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(14)].secondary.material, 9)
			wait()   
				fireServer("ChangeValue", v.Parent["skin" .. tostring(15)], 2)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(15)].material, 9)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(15)].secondary, 2)
				fireServer("ChangeValue", v.Parent["skin" .. tostring(15)].secondary.material, 9)
				wait(99999999999999999999999999999)
			end
		end)
		
		success(string.format("Gave (%s) SkinKit",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Gave (%s) SkinKit",Player.Name),"Blue")
	end 
end
},

["skins"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		spawn(function()
			for i, v in pairs(Player.playerstats.skins:GetChildren()) do
				fireServer("ChangeValue", v.Parent["skin" .. tostring(i)], math.random(1, 65))
				fireServer("ChangeValue", v.Parent["skin" .. tostring(i)].material, math.random(1, 12))
				fireServer("ChangeValue", v.Parent["skin" .. tostring(i)].secondary, math.random(1, 65))
				fireServer("ChangeValue", v.Parent["skin" .. tostring(i)].secondary.material, math.random(1, 12))
			end
		end)
		success(string.format("Gave (%s) Weapon Skins",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Gave (%s) Weapon Skins",Player.Name),"Blue")
	end 
end
},

["fillskins"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		local count = 0 
		for i = 1, 100 do
			local skin = Player.playerstats.skins["skin" .. tostring(i)]
			if skin.Value == 0 then
				count = count + 1
				local a,b,c,d = math.random(1,65),math.random(1,12),math.random(1,65),math.random(1,12)
				fireServer("ChangeValue", skin, a)
				fireServer("ChangeValue", skin.material, b)
				fireServer("ChangeValue", skin.secondary, c)
				fireServer("ChangeValue", skin.secondary.material, d)
			end
		end
		if count == 0 then
			invalid(string.format("(%s) Skin Slots Are Full",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s's) Skin Slots Are Full",Player.Name),"Red")
			return
		end 
		success(string.format("Filled (%s) Weapon Skins",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Filled (%s) Weapon Skins",Player.Name),"Blue")
	end
end
},

