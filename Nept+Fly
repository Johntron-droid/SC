["cleanserver"] = {
Callback = function(Mod)
	local VAmount = 0
	for _, v in pairs(Workspace:GetChildren()) do
		if v.Name == "C4Placed" or v.Name == "VS50Placed" or v.Name == "TM46Placed" or v.Name == "Fireplace" then
			fireServer("ChangeParent",v, nil)
			VAmount = VAmount + 1
		end
	end   

	for _, v in pairs(Workspace:GetChildren()) do
		if Lighting.LootDrops:FindFirstChild(v.Name) then
			fireServer("ChangeParent",v, nil)
			VAmount = VAmount + 1
		end
	end
	success(string.format("Cleaned Server | (%s) Items Deleted",VAmount))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Cleaned Server | (%s) Items Deleted",VAmount),"Blue")
end  
},

["cleanbases"] = {
Callback = function(Mod)
	local VAmount = 0
	for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do 
		if v.Name == "Floodlight" or v.Name == "LargeCrateOpen" or v.Name == "SmallCrateOpen" or v.Name == "Fireplace" then
			fireServer("ChangeParent", v, nil)
		elseif (v.Name == "Part" or v.Name == "Head" or v.Name == "Truss") then
			if table.find(Vector3Objects,v.Size) then
				fireServer("ChangeParent", v, nil)
				VAmount = VAmount + 1
			end
		end
	end
	if VAmount > 0 then
	success(string.format("Cleaned All Bases | (%s) Parts Removed",VAmount))
	_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Cleaned All Bases | (%s) Parts Removed",VAmount),"Blue")
	return end
	  if VAmount == 0 then
		_G_env["_G"].SendMessage(string.format("[!][Neptunium]: No Base Parts Were Found"),"Red")
		invalid("No Base Parts Were Found")
		return end
end  
},

["lootmod"] = {
	Callback = function(Mod,Value)
	Value = tonumber(Value)
	if noValue(Value) then return end
	  Workspace.Remote.ChangeLootModifier:FireServer(tonumber(Value))
		success(string.format("Set Loot Rate To (%s)",Value))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set Loot Rate To (%s)",Value),"Blue")
	end  
},

--   ~MapMods~
["nomap"] = {
Callback = function(Mod)
	fireServer("ChangeParent",game:GetService("Workspace"):FindFirstChild("Anchored Objects") ,nil)
	fireServer("ChangeParent",game:GetService("Workspace"):FindFirstChild("TerrainChunks") ,nil)
	success("Successfully Deleted Map.")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Deleted Map >:)"),"Blue")
end
},

["delbuilds"] = {
	Callback = function(Mod)
		local map 
		if Workspace:FindFirstChild("Anchored Objects") then
			map = Workspace
		end
		if not map then
			invalid(string.format("Map is currently set to nil"))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map is currently set to nil"),"Red")
		else
			local Towns
			if Workspace["Anchored Objects"]:FindFirstChild("Towns/Cities") then
				Towns = Workspace["Anchored Objects"]
				if not Towns then
					invalid(string.format("Towns/Cites are currently set to nil"))
					 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Towns/Cities are currently set to nil"),"Red")
					return
				end
				fireServer("ChangeParent", Workspace["Anchored Objects"]["Towns/Cities"], game.Lighting)
				success(string.format("Buildings have been deleted"))
				 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Buildings Haven Been Deleted"),"Blue")
			end
		end
	end
	
},


["addbuilds"] = {
	Callback = function(Mod)
		local map 
		if Workspace:FindFirstChild("Anchored Objects") then
			map = Workspace
		end
		if not map then
			invalid(string.format("Map is currently set to nil"))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map is currently set to nil"),"Red")
		else 
			local Towns
			if Lighting:FindFirstChild("Towns/Cities") then
				Towns = Lighting
				if not Towns then 
					invalid(string.format("Towns are still in Anchored Objects"))
					 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Towns are still in Anchored Objects"),"Red")
					return
				end
				fireServer("ChangeParent", game.Lighting:FindFirstChild("Towns/Cities"), Workspace["Anchored Objects"])
				success(string.format("Added buildings back to the map"))
				 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Added buildings back to the map"),"Blue")
			end
		end
	end
},


["snowmap"] = {
	Callback = function(Mod)
		if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 then 
			invalid("Coloring Is Disabled On This Map")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Coloring Is Disabled On This Map"),"Red")
			return
		end
		if not Workspace["Anchored Objects"] then 
			invalid("Map Is Deleted")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map Is Deleted"),"Red")
			return
		end
		if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
		success("Turning Reimagined Into Winter. Please wait...")
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Turning Reimagined Into Winter. Please Wait..."),"Blue")
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", Workspace["Anchored Objects"].Plates, "","","")
			Workspace["Anchored Objects"].Plates:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(Workspace["Anchored Objects"].Plates, 21, 6, 21,6)
			fireServer("ChangeParent", Workspace["Anchored Objects"].Plates:FindFirstChild("SecondaryColor"), nil)
			for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills:GetChildren()) do
				Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
				v:WaitForChild("SecondaryColor")
				Workspace.Remote.ColorGun:FireServer(v, 21, 6, 21,6)
				fireServer("VehichleLightsSet",v, "Snow",  0)
			end
			for _, v in pairs(Workspace["Anchored Objects"]["Plates"].Fields:GetDescendants()) do
				task.spawn(function()
					if v.Name == "HaybaleGroup" or v.Name  == "BigField" or v.Name == "SmallField" then
				Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
				v:WaitForChild("SecondaryColor")
				Workspace.Remote.ColorGun:FireServer(v, 21, 6, 21,6)
				fireServer("VehichleLightsSet",v, "Snow", 0)
			end
		end)
			end
					for _, v in pairs(Workspace["Anchored Objects"]["Trees/Foliage"]:GetDescendants()) do
						task.spawn(function()
					if v.Name == "Color" then
				Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
				v:WaitForChild("SecondaryColor")
				Workspace.Remote.ColorGun:FireServer(v, 21, 6, 21,6)
				fireServer("VehichleLightsSet",v, "Snow",  0)
			end
		end)
			end
				for _, v in pairs(Workspace["Anchored Objects"]["Trees/Foliage"]:GetDescendants()) do
				task.spawn(function()
					if v.Name == "Trunk" then
				Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
				v:WaitForChild("SecondaryColor")
				Workspace.Remote.ColorGun:FireServer(v, 13, 1, 13,6)
				fireServer("VehichleLightsSet",v, "Plastic", 0)
			end
		end)
			end
			for _, v in pairs(Workspace["Anchored Objects"].Plates:GetDescendants()) do
					if v.Name == "Roads" then
				Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
				v:WaitForChild("SecondaryColor")
				Workspace.Remote.ColorGun:FireServer(v, 36, 12, 36,12)
			end
		end
		for _, v in pairs(Workspace["Anchored Objects"].Plates:GetDescendants()) do
			if v.Name == "Dirt Roads" then
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
		v:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(v, 33, 1, 33,1)
	end
end
for _, v in pairs(Workspace["Anchored Objects"]["Trees/Foliage"].Rocks:GetChildren()) do
	task.spawn(function()
	if v.Name == "Rock1" or v.Name == "Rock2" or v.Name == "Rock3"  then
	Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
	v:WaitForChild("SecondaryColor")
	Workspace.Remote.ColorGun:FireServer(v, 60, 1, 60,1)
		fireServer("VehichleLightsSet",v, "Glass", 0)
	end
end)
end
			for _, v in pairs(Workspace["Anchored Objects"].Plates:GetChildren()) do
				if v.Name == "Roads" or v.Name == "Dirt Roads" or v.Name == "Fields" or v.Name == "Hills" then
					fireServer("ChangeParent",v,Workspace["Anchored Objects"].RoadSigns)
					fireServer("VehichleLightsSet",Workspace["Anchored Objects"].Plates, "Snow", 0)
				end
			end
			for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"].RadioStation.Spec_RadioTower1E:GetChildren()) do
				Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
				v:WaitForChild("SecondaryColor")
				Workspace.Remote.ColorGun:FireServer(v,21, 6, 21,6)
				fireServer("VehichleLightsSet",v, "Snow",  0)
			end
			wait(.55)
			for _, v in pairs(Workspace["Anchored Objects"].RoadSigns:GetChildren()) do
				if v.Name == "Roads" or v.Name == "Dirt Roads" or v.Name == "Fields" or v.Name == "Hills" then
					fireServer("ChangeParent",v,Workspace["Anchored Objects"].Plates)
				end
			end			
			for _, v in pairs(Workspace["Anchored Objects"]:GetDescendants()) do
				if v.Name == "SecondaryColor" then
					fireServer("ChangeParent", v, nil)
				end
			end
		end   
		success("Successfully Set " .. Workspace.mapname.Value .. "'s Map To Winter")
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Successfully Set " .. Workspace.mapname.Value .. "'s Map To Winter Theme"),"Blue")
	end
},

["spam"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notSpawned(Player.Name) then return end   
		for i, v in pairs (Player.playerstats.slots:GetChildren()) do
		if not v:FindFirstChild("ObjectID") then
				game.Workspace.Remote.AddClothing:FireServer("ObjectID", v, "PPSBSBaiBp", "", "")
				fireServer("ChangeValue", v, 1)
			elseif v:FindFirstChild("ObjectID") and v.ObjectID.Value ~= "PPSBSBaiBp" then
				fireServer("ChangeValue",v.ObjectID, "PPSBSBaiBp")
			end
		end
		wait()
		success(string.format("Made (%s) Items Drop.",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Made (%s) Items Drop",Player.Name),"Blue")
		 wait()
	end   
end
},

["desertmap"] = {
Callback = function(Mod)
	if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 then 
		invalid("Coloring Is Disabled On This Map")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Coloring Is Disabled On This Map"),"Red")
		return
	end
	if not Workspace["Anchored Objects"] then 
		invalid("Map Is Deleted")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map Is Deleted"),"Red")
		return
	end
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
	success("Turning Reimagined Into Desert. Please wait...")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Turning Reimagined Into Desert Theme. Please Wait..."),"Blue")
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", Workspace["Anchored Objects"].Plates, "","","")
		Workspace["Anchored Objects"].Plates:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(Workspace["Anchored Objects"].Plates, 32, 6, 32,6)
		fireServer("ChangeParent", Workspace["Anchored Objects"].Plates:FindFirstChild("SecondaryColor"), nil)
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 32, 6, 32,6)
		end
		for _, v in pairs(Workspace["Anchored Objects"]["Plates"].Fields:GetDescendants()) do
			task.spawn(function()
				if v.Name == "HaybaleGroup" or v.Name  == "BigField" or v.Name == "SmallField" then
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 52, 6, 52,6)
		end
	end)
		end
				for _, v in pairs(Workspace["Anchored Objects"]["Trees/Foliage"]:GetDescendants()) do
					task.spawn(function()
				if v.Name == "Color" or v.Name == "Trunk" then
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 58, 1, 58,1)
		end
	end)
		end
		wait()
			for _, v in pairs(Workspace["Anchored Objects"].Plates:GetDescendants()) do
			if v.Name ==  "Dirt Roads" or v.Name == "Roads" then		
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 52, 6, 52,6)
			end
		end	
		for _, v in pairs(Workspace["Anchored Objects"].Plates:GetChildren()) do
			if v.Name == "Roads" or v.Name == "Dirt Roads" or v.Name == "Fields" or v.Name == "Hills" then
				fireServer("ChangeParent",v,Workspace["Anchored Objects"].RoadSigns)
				fireServer("VehichleLightsSet",Workspace["Anchored Objects"].Plates, "Sand", 0)
			end
		end
		for _, v in pairs(Workspace["Anchored Objects"]["Trees/Foliage"].Rocks:GetChildren()) do
			task.spawn(function()
			if v.Name == "Rock1" or v.Name == "Rock2" or v.Name == "Rock3"  then
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 52, 6, 52,6)
				fireServer("VehichleLightsSet",Workspace["Anchored Objects"].Plates, "Sand", 0)
			end
		end)
		end
		for _, v in pairs(Workspace["Anchored Objects"]["Towns/Cities"].RadioStation.Spec_RadioTower1E:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v,32, 6, 32,6)
		end
		wait(.55)
		for _, v in pairs(Workspace["Anchored Objects"].RoadSigns:GetChildren()) do
			if v.Name == "Roads" or v.Name == "Dirt Roads" or v.Name == "Fields" or v.Name == "Hills" then
				fireServer("ChangeParent",v,Workspace["Anchored Objects"].Plates)
			end
		end			
		wait(.5)
		for _, v in pairs(Workspace["Anchored Objects"]:GetDescendants()) do
			if v.Name == "SecondaryColor" then
				fireServer("ChangeParent", v, nil)
			end
		end
	end   
	success("Successfully Desert " .. Workspace.mapname.Value .. "'s Map Colors.")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Successfully Set " .. Workspace.mapname.Value .. "'s Map To Desert Theme"),"Blue")
end
},

["map"] = {
Callback = function(Mod,Color,Texture)
	if not Color or tonumber(Color) == nil then
		invalid("Missing Or Invaild Color Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Color Input"),"Red")
		return
	end   
	if not Texture or tonumber(Texture) == nil then
		invalid("Missing Or Invaild Texture Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invaild Texture Input"),"Red")
		return
	end   
	Color = math.clamp(Color, 1, 65)
	Color = math.floor(Color + .5)
	Texture = math.clamp(Texture, 1, 12)
	Texture = math.floor(Texture + .5)
		if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 then --reborn
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", Workspace["TerrainChunks"], "","","")
		Workspace["TerrainChunks"]:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(Workspace["TerrainChunks"],Color, Texture, Color,Texture)
		fireServer("ChangeParent", Workspace["TerrainChunks"]:FindFirstChild("SecondaryColor"), nil)
		success("Setting Colors/Textures For " .. Workspace.mapname.Value.. "'s Map")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Setting Colors/Textures For " .. Workspace.mapname.Value.. "'s Map"),"Blue")
		for _, v in pairs(Workspace["TerrainChunks"]:GetChildren()) do
			task.spawn(function()
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v,Color, Texture, Color,Texture)                
		end)
	end
	end
	wait()
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then -- reimagined
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", Workspace["Anchored Objects"].Plates, "","","")
		Workspace["Anchored Objects"].Plates:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(Workspace["Anchored Objects"].Plates,Color, Texture, Color,Texture)
		fireServer("ChangeParent", Workspace["Anchored Objects"].Plates:FindFirstChild("SecondaryColor"), nil)
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills:GetChildren()) do
			task.spawn(function()
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v,Color, Texture, Color,Texture)
		end)
	end
		wait(.1)			
	end   
	if game.PlaceId == 290815963 or game.PlaceId == 1228677761 then --//Amend
		Workspace.Remote.AddClothing:FireServer("SecondaryColor",Workspace["Anchored Objects"].Plates.Plates, "","","")
		Workspace["Anchored Objects"].Plates.Plates:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(Workspace["Anchored Objects"].Plates.Plates,Color, Texture, Color,Texture)
		fireServer("ChangeParent", Workspace["Anchored Objects"].Plates.Plates:FindFirstChild("SecondaryColor"), nil)
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills.MediumHills:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v,Color, Texture, Color,Texture)
		end
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills.LargeHills:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v,Color, Texture, Color,Texture)
		end
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills.SmallHills:GetChildren()) do
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v,Color, Texture, Color,Texture)
		end
		wait(.1)
		for _, v in pairs(Workspace["Anchored Objects"]:GetDescendants()) do
			if v.Name == "SecondaryColor" then
				fireServer("ChangeParent", v, nil)
			end
		end
	end
	success("Colored/Textured " .. Workspace.mapname.Value.. "'s Map")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Colored/Textured " .. Workspace.mapname.Value.. "'s Map | Color Id (%s) | Texture Id (%s)",Color,Texture),"Blue")
end
},

["mapmat"] = {
Callback = function(Mod,Material,Value)
	Material = GetMaterials(Material)
	if not Material then
		invalid("Missing Or Invalid Material Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Material Input"),"Red")
		return
	end   
	Value = tonumber(Value)
	if noValue(Value) then return end
	Value = math.clamp(Value, 0, 1)
	if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 then --reborn
		for _, v in pairs(Workspace["TerrainChunks"]:GetChildren()) do
			fireServer("VehichleLightsSet",v, Material, Value)
			end
	end
	if not workspace["Anchored Objects"] then 
		invalid("Map Is Deleted")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map Is Deleted"),"Red")
		return
	end
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills:GetChildren()) do
			fireServer("VehichleLightsSet",v, Material, Value)
		end
		for _, v in pairs(Workspace["Anchored Objects"].Plates:GetChildren()) do
			if v.Name == "Roads" or v.Name == "Dirt Roads" or v.Name == "Fields" or v.Name == "Hills" then
				fireServer("ChangeParent",v,Workspace["Anchored Objects"].RoadSigns)
				fireServer("VehichleLightsSet",Workspace["Anchored Objects"].Plates, Material, Value)
			end
		end
		wait(2)
		for _, v in pairs(Workspace["Anchored Objects"].RoadSigns:GetChildren()) do
			if v.Name == "Roads" or v.Name == "Dirt Roads" or v.Name == "Fields" or v.Name == "Hills" then
				fireServer("ChangeParent",v,Workspace["Anchored Objects"].Plates)
			end
		end				
	end   
	if game.PlaceId == 290815963 or game.PlaceId == 1228677761 then --//Amend
		fireServer("VehichleLightsSet",Workspace["Anchored Objects"].Plates.Plates, Material, Value)
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills.MediumHills:GetChildren()) do
			fireServer("VehichleLightsSet",v, Material, Value)
		end
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills.LargeHills:GetChildren()) do
			fireServer("VehichleLightsSet",v, Material, Value)
		end
		wait()
		for _, v in pairs(Workspace["Anchored Objects"].Plates.Hills.SmallHills:GetChildren()) do
			fireServer("VehichleLightsSet",v, Material, Value)
		end
	end
	success(string.format("Success, Set " .. Workspace.mapname.Value.. "'s Material to (%s) and transparency (%s)",Material,Value))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set " .. Workspace.mapname.Value.. "'s Material to (%s) and transparency (%s)",Material,Value),"Blue")
end
},

