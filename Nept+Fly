["punish"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		fireServer("ChangeParent",Player.Character,Player)
		success(string.format("Punished (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Punished (%s)",Player.Name),"Blue")
	end  
end
},

["unpunish"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do     
		local Character = Player:FindFirstChild(Player.Name)
		if Workspace:FindFirstChild(Player.Name) then
			invalid(string.format("(%s) Not Punished Yet",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Not Punished Yet",Player.Name),"Red")
			return
		end
		fireServer("ChangeParent",Character, workspace)
		success(string.format("Unpunished (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Unpunished (%s)",Player.Name),"Blue")
	end  
end
},

["trip"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		workspace.Remote.HurtZombie:FireServer(Player.Character)
		success(string.format("Tripped (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Tripped (%s)",Player.Name),"Blue")
	end 
end
},

--Spawning

["spawn"] = {
Callback = function(Mod,Player,Item,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Item = GetItemName(Item)
		if not Item then
			invalid("Missing Or Invalid Item Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Item Input"),"Red")
			return
		end   
		Value = tonumber(Value)
		if noValue(Value) then return end
		Value = math.clamp(Value, 0, 45)
		Value = math.floor(Value + .5)
		for i = 1, Value do 
			spawn(function()
				Clone(Player,Item,Lighting.LootDrops,Vector3.new(math.random(-5, 5),0,math.random(-5, 5)))
			end)
		end
		success(string.format("Spawned - (%s) - (%s) - (%s)",Player.Name,Item,Value))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Spawned - (%s) - (%s) - (%s)",Player.Name,Item,Value),"Blue")
	end
end
},

["m14kit"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		for i = 1, #KitsList["M14"] do
			local Amount = 1
			local Item = KitsList["M14"][i]
			if typeof(Item) == "table" then
				Amount = Item[2]
				Item = Item[1]
			end
			for a = 1, Amount do
				spawn(function()
					Clone(Player,Item,Lighting.LootDrops,Vector3.new(math.random(-5, 5),0,math.random(-5, 5)),true)
				end)
			end
		end
		success(string.format("Gave A M14 Kit To (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Gave M14 Kit To (%s)",Player.Name),"Blue")
	end
end
},

