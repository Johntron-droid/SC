if tostring(Key) == "`" then
Type:CaptureFocus()
end
end)

function notLoaded(Player)
if not Workspace:FindFirstChild(Player) then
invalid(Player .. " Is Either Not Loaded In Or Punished")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Is Either Not Loaded In Or Punished",Player),"Red")
return true
end 
end

function notSpawned(Player)
if not Players[Player].Character.IsSpawned.Value == true then
invalid(Player .. " Is Not Spawned In Yet")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Is Not Spawned In yet",Player),"Red")
return true
end 
end

function noHumanoid(Player)
if not Players[Player].Character:FindFirstChild("Humanoid") then
invalid(Player .. "'s Humanoid Is Missing Or Is Removed")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Is Either Not Loaded In Or Punished",Player),"Red")
return true
end 
end


--offlist stuff
meta = getrawmetatable(game)
if getgenv().MetaSet == nil or getgenv().MetaSet == true then
local ni = meta.__newindex
getgenv().MetaSet = {["ni"] = meta.__newindex}
end

local ni = getgenv().MetaSet["ni"]
setreadonly(meta, false)
meta.__newindex = newcclosure(function(rc, ind, id)
if ind == "Parent" and ((rc:IsA("Player") and not id and rc.Parent ~= nil) or rc:IsA("RemoteEvent") or rc:IsA("RemoteFunction")) then
return
end
return ni(rc, ind, id)
end)

offList = false
LPlr = game.Players.LocalPlayer
Players.PlayerAdded:Connect(function(Player)
if offList == true then
fireServer("ChangeParentLocal", LPlr)
end
end)
