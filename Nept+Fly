	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", Workspace["Anchored Objects"]["Trees/Foliage"], "","","")
		Workspace["Anchored Objects"]["Trees/Foliage"]:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(Workspace["Anchored Objects"]["Trees/Foliage"]["Trees"],Color, Texture, Color,Texture)
		for _, v in pairs(Workspace["Anchored Objects"]["Trees/Foliage"]["Trees"]:GetDescendants()) do
		task.spawn(function()
		if v.Name == "Color" or v.Name == "Trunk"  then
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v,Color, Texture, Color,Texture)
			end
		end)
	end
	for _, v in pairs(Workspace["Anchored Objects"]:GetDescendants()) do
		if v.Name == "SecondaryColor" then
			fireServer("ChangeParent", v, nil)
		end
	end
end
	
	success("Colored/Textured " .. Workspace.mapname.Value.. "'s Trees")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Colored/Textured " .. Workspace.mapname.Value.. "'s Trees | Color Id (%s) | Texture Id (%s)",Color,Texture),"Blue")
end
