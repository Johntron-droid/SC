["reload"] = {
Callback = function(Mod,Player,Weapon,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Weapon = GetWeaponName(Weapon)
		if noWeapon(Weapon) then return end
		Value = tonumber(Value)
		if noValue(Value) then return end
		Value = math.clamp(Value, -99999999999999, 99999999999999) 
		Value = math.floor(Value + .5)
		if notSpawned(Player.Name) then return end
		local Can = false
		for _, WeaponObj in pairs(Player.Backpack:GetChildren()) do
			if WeaponObj.Name == Weapon then
				Can = true
			end
		end
		if not Can then
			invalid(string.format("(%s) does not have a (%s)",Player.Name,Weapon))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) does not have a (%s)",Player.Name,Weapon),"Red")
			return 
		end
		fireServer("ChangeValue", Player.Backpack[Weapon].Stats.Reload, Value)
		success(string.format("Set (%s's) (%s) Reload Speed To (%s)",Weapon,Value))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s's) (%s) Reload Speed To (%s)",Weapon,Value),"Blue")
	end   
end
},

["oneshot"] = {
Callback = function(Mod,Weapon)
	Weapon = GetWeaponName(Weapon)
	if noWeapon(Weapon) then return end
	local Can = false
	for _, WeaponObj in pairs(Client.Backpack:GetChildren()) do
		if WeaponObj.Name == Weapon then
			Can = true
		end
	end
	if not Can then
		invalid(string.format("You do not have a (%s)",Weapon))
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: You do not have a (%s)",Weapon),"Red")
		return 
	end
	Gun = Client.Backpack[Weapon]
	local Name = Gun.Name
	Workspace.Remote.Detonate:FireServer(Gun)
	wait(.16)
	Gun.Name = Name
	fireServer("ChangeValue", Gun.Stats.Damage,  _G_env._G.Obfuscate(9e9))
	success(string.format("Made Your (%s) One Shot",Weapon))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Made Your (%s) One Shot",Weapon),"Blue")
end 
},

["wallbang"] = {
Callback = function()
	if Spawned() then return end
	Wallbang = true
	success("Turned On Wallbang")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Turned On Wallbang"),"Blue")
end 
},

["unwallbang"] = {
Callback = function()
	if Spawned() then return end
	Wallbang = false
	success("Turned On Wallbang")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Turned Off Wallbang"),"Blue")
end 
},

["add"] = {
Callback = function(Mod,Player,Weapon,Attachment)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notSpawned(Player.Name) then return end
		Weapon = GetWeaponName(Weapon)
		if noWeapon(Weapon) then return end
		Attachment = GetAttachments(Attachment)
		if not Attachment then
			invalid("Missing Or Invalid Attachment Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Attachment Input"),"Red")
			return
		end   
		local Can = false
		for _, WeaponObj in pairs(Player.Backpack:GetChildren()) do
			if WeaponObj.Name == Weapon then
				Can = true
			end
		end
		if not Can then
			invalid(string.format("(%s) does not have a (%s)",Player.Name,Weapon))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) does not have a (%s)",Player.Name,Weapon),"Red")
			return 
		end

		local WeaponType = "primary"
		if Player.Backpack[Weapon]:FindFirstChild("secondarymarker") then
			WeaponType = "secondary"
		end

		if not Player.playerstats.attachments[WeaponType][AttachmentList[Attachment][1]]:FindFirstChild("ObjectID") then
			Workspace.Remote.AddClothing:FireServer("ObjectID",Player.playerstats.attachments[WeaponType][AttachmentList[Attachment][1]], "","","")
		end
		Player.playerstats.attachments[WeaponType][AttachmentList[Attachment][1]]:WaitForChild("ObjectID")
		fireServer("ChangeValue", Player.playerstats.attachments[WeaponType][AttachmentList[Attachment][1]],1)
		fireServer("ChangeValue", Player.playerstats.attachments[WeaponType][AttachmentList[Attachment][1]].ObjectID,  _G_env._G.Obfuscate((AttachmentList[Attachment][2])))
		Workspace.Remote.AddClothing:FireServer("anything lol",Player.playerstats.attachments[WeaponType][AttachmentList[Attachment][1]],"","","")
		wait()
		fireServer("ChangeParent", Player.playerstats.attachments[WeaponType][AttachmentList[Attachment][1]]:WaitForChild("anything lol"),nil)

		success(string.format("Added a (%s) on (%s's) (%s)",Attachment,Player.Name,Weapon))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Added a (%s) on (%s's) (%s)",Attachment,Player.Name,Weapon),"Blue")
	end
end
},

["rainbowtools"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
	if notLoaded(Player.Name) then return end
	if notSpawned(Player.Name) then return end
	spawn(function()
		while true do
			if RainbowGunEnabled then
				local Weld = Player.Character["Right Arm"]:FindFirstChild("Weld")
				if Weld and Weld.Part1 ~= nil then
					Workspace.Remote.AddClothing:FireServer("SecondaryColor", Weld.Part1.Parent, "","","")
					Weld.Part1.Parent:WaitForChild("SecondaryColor")
					local Random = math.random(16, 20)
					workspace.Remote.ColorGun:FireServer(Weld.Part1.Parent, Random , 1, Random , 1)
					fireServer("VehichleLightsSet",Weld.Part1.Parent, "Neon", 0)
					fireServer("ChangeParent",Weld.Part1.Parent:FindFirstChild("SecondaryColor"), nil)
				end
			end
			wait(0.1)
		end
	end)
	RainbowGunEnabled = true
	Event = Player.Character.Humanoid.Died:Connect(function()
		RainbowGunEnabled = false
		Event:Disconnect()
	end)

	
	success(string.format("Enabled (%s) Rainbow Tools",Player.Name))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Tools To Rainbow",Player.Name),"Blue")
end 
end
},

["unrainbowtools"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
	if notLoaded(Player.Name) then return end
	if notSpawned(Player.Name) then return end
	RainbowGunEnabled = false
	success(string.format("Disabled (%s) Rainbow Tools",Player.Name))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled (%s) Rainbow Tools",Player.Name),"Blue")
end 
end
},

["rainbowbag"] = {
Callback = function(Mod,Player)
   local Playerlist = returnPlayers(Mod,Player) 
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		local BackPackObject
		for BackpackName in pairs(BackpackList) do
			if Player.Character:FindFirstChild(BackpackName) then
				BackPackObject = Player.Character[BackpackName]
			end    
		end
		if not BackPackObject then
			invalid(Player.Name .. " Does Not Have A Backpack")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Does Not Have A Backpack",Player.Name),"Red")
			return
		end    
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", BackPackObject, "","","")
		BackPackObject:WaitForChild("SecondaryColor")
	spawn(function()
	while true do 
	if RainbowBagEnabled then
			local Random = math.random(16, 20)
		Workspace.Remote.ColorGun:fireServer(BackPackObject, Random, 1, Random, 1)
		fireServer("VehichleLightsSet",BackPackObject, "Neon", 0)
	   end  
	wait(0.1)
	end
end)
	RainbowBagEnabled = true
	Event = Player.Character.Humanoid.Died:Connect(function()
		RainbowBagEnabled = false
		Event:Disconnect()
		end)
		success(string.format("Set (%s) Bag To Rainbow",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Bag To Rainbow",Player.Name),"Blue")
end 
end
},

