["clone"] = {
Callback = function(Mod,Location,Player)
	Location = GetLocation(Location)
	if noLocation(Location) then return end
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notSpawned(Player.Name) then return end
		if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 then 
			invalid("Cloning Towns/Citys Is Disabled On This Map")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Cloning Towns/Citys Is Disabled On This Map"),"Red")
			return
		end
		if not workspace["Anchored Objects"] then 
			invalid("Map Is Deleted")
			return
		end
		if not workspace["Anchored Objects"]["Towns/Cities"][Location]:IsA("Model") then 
			invalid("Invalid Location")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Invalid Location"),"Red")
			return 
		end
		spawn(function()
			Clone(Player,Location,Workspace["Anchored Objects"]["Towns/Cities"],Vector3.new(math.random(-30, 30),0,math.random(-30, 30)))
		end)
		success(string.format("Cloned (%s) To (%s)",Location,Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Cloned (%s) To (%s)",Location,Player.Name),"Blue")
	end
end
},

["cloneheli"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notSpawned(Player.Name) then return end
		spawn(function()
			Clone(Player,"HeliCrash", Workspace, Vector3.new(math.random(-30, 30),0,math.random(-30, 30)))
		end)
		success(string.format("Cloned The HeliCrash To (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Cloned HeliCrash To (%s)",Player.Name),"Blue")
	end
end
},

["clonespawn"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notSpawned(Player.Name) then return end
		spawn(function()
			Clone(Player,"SpawnPlate", ReplicatedStorage, Vector3.new(math.random(-30, 30),0,math.random(-30, 30)))
		end)
		success(string.format("Cloned The Spawn Island To (%s)",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Cloned The Spawn Island To (%s)",Player.Name),"Blue")
	end
end
},

--Vehicles

["vcolor"] = {
Callback = function(Mod,Vehicle,Color)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	if not Color or tonumber(Color) == nil then
		invalid("Missing Or Invalid Color Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Color Input"),"Red")
		return
	end   

	Color = math.clamp(Color, 1, 65)
	Color = math.floor(Color + .5)
	if Workspace.Vehicles[Vehicle].Stats.Armor.Value > 0 then
		for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do     
			if  v.Name == "Special" or v.Name == "Color" or v.Name == "Black" or v.Name == "White" or v.Name == "Details" or v.Name == "RoofLights" or v.Name == "DoorHandles" or v.Name == "FrontLights" or v.Name == "Grill" or v.Name == "Interior" or v.Name == "Frame" or v.Name == "LMWheel" or v.Name == "LBWheel" or v.Name == "Black" or v.Name == "LFWheel" or v.Name == "RFWheel" or v.Name == "RMWheel" or v.Name == "RBWheel" then
				Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
				v:WaitForChild("SecondaryColor")
				workspace.Remote.ColorGun:FireServer(v,Color, 2, Color,2)
				fireServer("VehichleLightsSet",v, "Plastic", 0)
			end
		end
	else
		for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do     
			if  v.Name == "Special" or v.Name == "Color" or v.Name == "Black" or v.Name == "White" or v.Name == "Details" or v.Name == "RoofLights" or v.Name == "DoorHandles" or v.Name == "FrontLights" or v.Name == "Grill" or v.Name == "Interior" or v.Name == "Frame" or v.Name == "LMWheel" or v.Name == "LBWheel" or v.Name == "Black" or v.Name == "LFWheel" or v.Name == "RFWheel" or v.Name == "RMWheel" or v.Name == "RBWheel" then
				Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
				v:WaitForChild("SecondaryColor")
				workspace.Remote.ColorGun:FireServer(v, Color, 1, Color,1)
				fireServer("VehichleLightsSet",v, "Plastic", 0)
			end
		end
	end
	if Vehicle == "PoliceCar" then
		success(string.format("Vehicle Colored (%s) | Color id (%s)",Vehicle,Color))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Vehicle Colored (%s) | Color id (%s)",Vehicle),"Blue")
		return
	end  
	for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do     
		if  v.Name == "ArmorSkirt" then
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			workspace.Remote.ColorGun:FireServer(v, Color , 2, Color,2)
			fireServer("VehichleLightsSet",v, "Metal", 0)
		end
	end
	for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do
		if  v.Name == "SecondaryColor" then
			fireServer("ChangeParent",v, nil)
		end
	end
	success(string.format("Vehicle Colored (%s) | Color id (%s)",Vehicle,Color))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Vehicle Colored (%s) | Color id (%s)",Vehicle,Color),"Blue")
end  
},

["vmat"] = {
Callback = function(Mod,Vehicle,Material)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end

	Material = GetMaterials(Material)
	if not Material then
		invalid("Missing Or Invalid Material Input")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Material Input"),"Red")
		return
	end   

	if Workspace.Vehicles[Vehicle].Essentials.Color:FindFirstChild("ArmorSkirt") then
		fireServer("ChangeParent", Workspace.Vehicles[Vehicle].Essentials.Color:FindFirstChild("ArmorSkirt"), Workspace.Vehicles[Vehicle])
		fireServer("VehichleLightsSet",Workspace.Vehicles[Vehicle].Essentials.Color:FindFirstChild("ArmorSkirt"), Material, 0)
	end   
	for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do
		if  v.Name == "Special" or v.Name == "Color" or v.Name == "Black" or v.Name == "White" or v.Name == "LMWheel" or v.Name == "LBWheel" or v.Name == "Black" or v.Name == "LFWheel" or v.Name == "RFWheel" or v.Name == "RMWheel" or v.Name == "RBWheel" then
			fireServer("VehichleLightsSet",v, Material, 0)
		end
	end
	wait()
	Workspace.Vehicles[Vehicle]:WaitForChild("ArmorSkirt")
	fireServer("ChangeParent", Workspace.Vehicles[Vehicle]:FindFirstChild("ArmorSkirt"),Workspace.Vehicles[Vehicle].Essentials.Color)
	success(string.format("Set (%s's) material to (%s)",Vehicle,Material))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) material to (%s)",Vehicle,Material),"Blue")
end  
},

["vtrans"] = {
Callback = function(Mod,Vehicle,Value)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	Value = tonumber(Value)
	if noValue(Value) then return end
	Value = math.clamp(Value, 0, 1)
	if Workspace.Vehicles[Vehicle].Essentials.Color:FindFirstChild("ArmorSkirt") then
		fireServer("ChangeParent",Workspace.Vehicles[Vehicle].Essentials.Color:FindFirstChild("ArmorSkirt"),Workspace.Vehicles[Vehicle])
		fireServer("VehichleLightsSet",Workspace.Vehicles[Vehicle].Essentials.Color:FindFirstChild("ArmorSkirt"), "Plastic", Value)
		wait(.15)
		fireServer("ChangeParent", Workspace.Vehicles[Vehicle]:FindFirstChild("ArmorSkirt"),Workspace.Vehicles[Vehicle].Essentials.Color)
	end   
	for _, v in pairs(Workspace.Vehicles[Vehicle]:GetDescendants()) do
		if v.Name == "Color" or v.Name == "Black" or v.Name == "White" or v.Name == "White" or v.Name == "Details" or v.Name == "Interior" or v.Name == "Special" then
			fireServer("VehichleLightsSet",v, "Plastic", Value)
		end
	end
	success(string.format("Set (%s) transparency To (%s)",Vehicle,tostring(Value)))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) transparency To (%s)",Vehicle,tostring(Value)),"Blue")
end  
},

["vfix"] = {
Callback = function(Mod,Vehicle)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	if Vehicle == "Firetruck" or Vehicle == "Ambulance" then
		fireServer("HullArmorSet",Workspace.Vehicles[Vehicle]:FindFirstChild("ArmorSkirt", true))
	else        
		fireServer("HullArmorSet",Workspace.Vehicles[Vehicle]:FindFirstChild("ArmorSkirt", true),Workspace.Vehicles[Vehicle]:FindFirstChild("Color",true),Workspace.Vehicles[Vehicle]:FindFirstChild("Special",true))
	end
	for _, v in next,Workspace.Vehicles[Vehicle].Wheels:GetChildren() do 
		fireServer("WheelVisibleSet", v,'Armored')
	end
	fireServer("WindowArmorSet", Workspace.Vehicles[Vehicle].Essentials.Details:FindFirstChild("Windows") ,'Ballistic')
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Hull, 250)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Hull.Max, 250)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Engine, 150)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Engine.Max, 150)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Tank, 150)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Tank.Max, 150)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Armor, 350)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Armor.Max, 350)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Fuel, 9999999)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Fuel.Max, 9999999)
	success(string.format("Repaired (%s)",Vehicle))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Repaired (%s)",Vehicle),"Blue")
end  
},

["vfuel"] = {
Callback = function(Mod,Vehicle)
	Vehicle = GetVehiclesName(Vehicle)
	if noVehicle(Vehicle) then return end
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Fuel, 9999999)
	fireServer("ChangeValue", Workspace.Vehicles[Vehicle].Stats.Fuel.Max, 9999999)
	success(string.format("Refueled (%s)",Vehicle))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Refueled (%s)",Vehicle),"Blue")
end  
},

