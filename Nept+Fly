["firerate"] = {
Callback = function(Mod,Player,Weapon,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Weapon = GetWeaponName(Weapon)
		if noWeapon(Weapon) then return end
		Value = tonumber(Value)
		if noValue(Value) then return end
		Value = math.clamp(Value, 1, 99999999999999) 
		Value = math.floor(Value + .5)
		if notSpawned(Player.Name) then return end
		local Can = false
		for _, WeaponObj in pairs(Player.Backpack:GetChildren()) do
			if WeaponObj.Name == Weapon then
				Can = true
			end
		end
		if not Can then
			invalid(string.format("(%s) does not have a (%s)",Player.Name,Weapon))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) does not have a (%s)",Player.Name,Weapon),"Red")
			return 
		end
		fireServer("ChangeValue", Player.Backpack[Weapon].Stats.Action, 1)
		fireServer("ChangeValue", Player.Backpack[Weapon].Stats.Action.Original, 3)
		fireServer("ChangeParent",Player.Backpack[Weapon].Shooter,Player)
		wait()
		if not Player.Backpack[Weapon].Stats.Action:FindFirstChild("Rate") then
			Workspace.Remote.AddClothing:FireServer("Rate", Player.Backpack[Weapon].Stats.Action, "","","")
		end   
		Player.Backpack[Weapon].Stats.Action:WaitForChild("Rate")
		fireServer("ChangeValue", Player.Backpack[Weapon].Stats.Action.Rate,  _G_env._G.Obfuscate(Value))
		wait(.1)
		fireServer("ChangeParent",Player.Shooter,Player.Backpack[Weapon])
		success(string.format("Set (%s's) (%s) FireRate To (%s)",Player.Name,Weapon, tostring(Value)))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s's) (%s) FireRate To (%s)",Player.Name,Weapon, tostring(Value)),"Blue")
	end 
end
},

["qreload"] = {
Callback = function(Mod,Player,Weapon)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Weapon = GetWeaponName(Weapon)
		if noWeapon(Weapon) then return end
		if notSpawned(Player.Name) then return end
		local Can = false
		for _, WeaponObj in pairs(Player.Backpack:GetChildren()) do
			if WeaponObj.Name == Weapon then
				Can = true
			end
		end
		if not Can then
			invalid(string.format("(%s) does not have a (%s)",Player.Name,Weapon))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) does not have a (%s)",Player.Name,Weapon),"Red")
			return 
		end
		fireServer("ChangeValue", Player.Backpack[Weapon].Stats.Reload, -99999999999999)
		success(string.format("Made (%s's) (%s) Reload Speed Fast",Player.Name,Weapon))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s's) (%s) Reload Speed To Instant",Player.Name,Weapon),"Blue")
	end   
end
