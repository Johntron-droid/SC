["fixcolors"] = {
Callback = function() 
	if not workspace.Remote:GetChildren()[141] and not workspace.Remote:GetChildren()[142] then
		invalid("Color Cmds Are Working")
	  _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Color Cmds Are Working"),"Red")
	 return end
	 if workspace.Remote:GetChildren()[142] or workspace.Remote:GetChildren()[141] then
		success("Fixed Color Cmds!")
		_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Fixed Color Cmds! | Removed Dupe ColorGun Remotes"),"Blue")
	 end
   if workspace.Remote:GetChildren()[142] then
	workspace.Remote:GetChildren()[142]:Destroy() 
   end
  if workspace.Remote:GetChildren()[141] then
	workspace.Remote:GetChildren()[141]:Destroy()
  end
	end
},

["fixwindows"] = {
Callback = function(Mod) 
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
		for _, v in pairs(Workspace["Anchored Objects"]:GetDescendants()) do
		if v.Name == "Windows" or v.Name == "Window" then
			fireServer("VehichleLightsSet",v, "SmoothPlastic", 0.3)
		end
			end
				end
			
	success("Fixed Map's Windows Transparency")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Fixed Map's Windows Transparency"),"Blue")
end
},

["detect"] = {
Callback = function()
	wait(0.3)
	for i, v in pairs(game.Workspace:GetDescendants()) do
		if v.Name == "Famine" and v.Parent:FindFirstChild("Animate") and not game.Players:FindFirstChild(v.Parent.Name) then
			function Callback(answer)
				if answer == "Kill" then
					fireServer("ChangeParent", v.Parent.Head)
					_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Killed Offlist Player (%s)",v.Parent.Name),"Blue")
					success(string.format("Killed Offlist Player(%s)",v.Parent.Name))
				end
			end
			Bindable = Instance.new("BindableFunction")
			Bindable.OnInvoke = Callback
			game:GetService("StarterGui"):SetCore("SendNotification",{
				Title = "    [Neptunium]", 
				Text = "(".. v.Parent.Name .. ") Is Invisible On PlayerList!", 
				Icon = "https://www.roblox.com/asset-thumbnail/image?assetId=376938026&width=420&height=420&format=png";
				Button1 = "Kill";
				Duration = 8,
				Callback = Bindable,
			})
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Is Invisible On The Playerlist!",v.Parent.Name),"Yellow")
			 warn(string.format("[!][Detection]: (%s) Is Invisible On The Playerlist!",v.Parent.Name))
		end
	end
	for _,v in pairs(Players:GetPlayers(val)) do
		if v ~= LPlr then
	if CheckNumber(v.playerstats.Hunger.Value) == nil or CheckNumber(v.playerstats.Thirst.Value) == nil then
		if not val then
		function Callback(answer)
			if answer == "Kick" then
				KickPlayer(v, .25)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Kicked (%s)",v.Name),"Blue")
				success(string.format("Kicked (%s)",v.Name))
			elseif answer == "Kill" then
				if deadPlayer(v.Name) then return end
				fireServer("ChangeParent", v.Character:FindFirstChild("Head"), nil)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Killed (%s)",v.Name),"Blue")
				success(string.format("Killed (%s)",v.Name))
			end
		end
		Bindable = Instance.new("BindableFunction")
		Bindable.OnInvoke = Callback
		game:GetService("StarterGui"):SetCore("SendNotification",{
			Title = "    [Neptunium]", 
			Text = "(".. v.Name .. ") Has Infinite Vitals!", 
			Icon = "https://www.roblox.com/asset-thumbnail/image?assetId=376938026&width=420&height=420&format=png";
			Button1 = "Kill";
			Button2 = "Kick",
			Duration = 8,
			Callback = Bindable,
		})
		warn(string.format("[!][Detection]: (%s) Has Infinite Vitals!",v.Name))
		 _G_env["_G"].SendMessage(string.format("[!][D][Neptunium]: (%s) Has Infinite Vitals!",v.Name),"Yellow")
	end
	end
	if v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid:FindFirstChild("DefenseMultiplier") and tonumber(v.Character.Humanoid.DefenseMultiplier.Value) <= 0 then
		if not val then
		function Callback(answer)
			if answer == "Kick" then
				KickPlayer(v, .25)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Kicked (%s)",v.Name),"Blue")
				success(string.format("Kicked (%s)",v.Name))
			elseif answer == "Kill" then
				if deadPlayer(v.Name) then return end
				fireServer("ChangeParent", v.Character:FindFirstChild("Head"), nil)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Killed (%s)",v.Name),"Blue")
				success(string.format("Killed (%s)",v.Name))
			end
		end
		Bindable = Instance.new("BindableFunction")
		Bindable.OnInvoke = Callback
		game:GetService("StarterGui"):SetCore("SendNotification",{
			Title = "    [Neptunium]", 
			Text = "(".. v.Name .. ") Is Godded!", 
			Icon = "https://www.roblox.com/asset-thumbnail/image?assetId=376938026&width=420&height=420&format=png";
			Button1 = "Kill";
			Button2 = "Kick",
			Duration = 8,
			Callback = Bindable,
		})
		 _G_env["_G"].SendMessage(string.format("[!][D][Neptunium]: (%s) Is Godded!",v.Name),"Yellow")
		 warn(string.format("[!][Detection]: (%s) Is Pk Godded!",v.Name))
	end
end
	--
	if not v.Character:FindFirstChild("Humanoid") then
		if not val then
		function Callback(answer)
			if answer == "Kick" then
				KickPlayer(v, .25)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Kicked (%s)",v.Name),"Blue")
				success(string.format("Kicked (%s)",v.Name))
			elseif answer == "Kill" then
				if deadPlayer(v.Name) then return end
				fireServer("ChangeParent", v.Character:FindFirstChild("Head"), nil)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Killed (%s)",v.Name),"Blue")
				success(string.format("Killed (%s)",v.Name))
			end
		end
		Bindable = Instance.new("BindableFunction")
		Bindable.OnInvoke = Callback
		game:GetService("StarterGui"):SetCore("SendNotification",{
			Title = "    [Neptunium]", 
			Text = "(".. v.Name .. ") Is Humanoid Godded!", 
			Icon = "https://www.roblox.com/asset-thumbnail/image?assetId=376938026&width=420&height=420&format=png";
			Button1 = "Kill";
			Button2 = "Kick",
			Duration = 8,
			Callback = Bindable,
		})
		 _G_env["_G"].SendMessage(string.format("[!][D][Neptunium]: (%s) Is Humanoid Godded!",v.Name),"Yellow")
		 warn(string.format("[!][Detection]: (%s) Is Humanoid Godded!",v.Name))
	end
end

	if tonumber(string.sub(tostring(v.Character.Humanoid.Health), 1, 1)) == nil then
		if not val then
		function Callback(answer)
			if answer == "Kick" then
				KickPlayer(v, .25)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Kicked (%s)",v.Name),"Blue")
				success(string.format("Kicked (%s)",v.Name))
			elseif answer == "Kill" then
				if deadPlayer(v.Name) then return end
				fireServer("ChangeParent", v.Character:FindFirstChild("Head"), nil)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Killed (%s)",v.Name),"Blue")
				success(string.format("Killed (%s)",v.Name))
			end
		end
		Bindable = Instance.new("BindableFunction")
		Bindable.OnInvoke = Callback
		game:GetService("StarterGui"):SetCore("SendNotification",{
			Title = "    [Neptunium]", 
			Text = "(".. v.Name .. ") Has Infinite Health!", 
			Icon = "https://www.roblox.com/asset-thumbnail/image?assetId=376938026&width=420&height=420&format=png";
			Button1 = "Kill";
			Button2 = "Kick",
			Duration = 8,
			Callback = Bindable,
		})
		 _G_env["_G"].SendMessage(string.format("[!][D][Neptunium]: (%s) Has Infinite Health!",v.Name),"Yellow")
		 warn(string.format("[!][Detection]: (%s) Has Infinite Health!",v.Name))
	end
end
	if v.playerstats:FindFirstChild("meow") then
		if not val then
		function Callback(answer)
			if answer == "Kick" then
				KickPlayer(v, .25)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Kicked (%s)",v.Name),"Blue")
				success(string.format("Kicked (%s)",v.Name))
			elseif answer == "Kill" then
				if deadPlayer(v.Name) then return end
				fireServer("ChangeParent", v.Character:FindFirstChild("Head"), nil)
				_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Killed (%s)",v.Name),"Blue")
				success(string.format("Killed (%s)",v.Name))
			end
		end
		Bindable = Instance.new("BindableFunction")
		Bindable.OnInvoke = Callback
		game:GetService("StarterGui"):SetCore("SendNotification",{
			Title = "[Neptunium][Dev]", 
			Text = "(".. v.Name .. ") Is Using Neptunium v.n5!", 
			Icon = "https://www.roblox.com/asset-thumbnail/image?assetId=376938026&width=420&height=420&format=png";
			Button1 = "Kill";
			Button2 = "Kick",
			Duration = 8,
			Callback = Bindable,
		})
		warn(string.format("[!][Detection]: (%s) Is Using Neptunium v.n5",v.Name))
		_G_env["_G"].SendMessage(string.format("[!][D][Neptunium]: (%s) Is Using Neptunium v.n5 | Dev Alert",v.Name),"Yellow")
   end
end
end
end
wait(0.1)
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: Detection Check Complete!"),"Blue")
	success("Detection Check Complete!")
end
},

["votesystem"] = {
Callback = function()
if votekick == true then
invalid("VoteKick System Already Active")
_G_env["_G"].SendMessage(string.format("[!][Neptunium]: VoteKick System Already Active"),"Red")
end
if votekick == false then
loadstring(game:HttpGet("https://raiidev.xyz/raiihub/neptbackend/apocvotekick.lua"))();
	success("Activating Raii's VoteKick System")
	_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Activating Raii's VoteKick System"),"Blue")
	game.Workspace.Remote.Chat:FireServer("Global", "[Neptunium]: Activating Raii's VoteKick System")
	votekick = true
end	
end 
},

["silentaim"] = {
Callback = function()
	if _G.silentaim == true then
		invalid("Silent Aim is already enabled")
		_G_env["_G"].SendMessage(string.format("[!][Neptunium]: Silent Aim Is Already Enabled"),"Red")
	return end
	_G.silentaim = true
	if _G.silentaim == true then
		silentaim()
	end
	success("Silent Aim : Enabled")
	_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Silent Aim : Enabled | Radius (1000M)"),"Blue")
	
end 
},

["unsilentaim"] = {
Callback = function()
	if _G.silentaim == false then
		invalid("Silent Aim is already disabled")
		_G_env["_G"].SendMessage(string.format("[!][Neptunium]: Silent Aim Is Already Disabled"),"Red")
	return end
	_G.silentaim = false
	success("Silent Aim Disabled")
	_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Silent Aim : Disabled"),"Blue")
end 
},

["ws"] = {
Callback = function(Mod,Value) 
	if Loaded() then return end
	if Humanoid() then return end
	if Dead() then return end
	Value = tonumber(Value)
	if noValue(Value) then return end
	Value = math.floor(Value + .5)
	SkyboxRenderMode()
	 _G_env._G.walkbase = tonumber(Value)
	if Client.PlayerGui.HitEqualsYouDie:FindFirstChild("JumpLimiter") then
		MessageBox.Title.Text = "Disable Fall Damage?"
		MessageBox.Message.Text = "Neptunium's suggestion is for you to disable fall damage, so when running off hill or mountains you don't hit ground too hard"
		MessageBox.Visible = true
		Buttons.YesNo.Visible = true
	end
	success(string.format("Set your walkspeed to (%s)",tostring(Value)))
	_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set your walkspeed to (%s)",tostring(Value)),"Blue")
end
},

["jp"] = {
Callback = function(Mod,Value) 
	if Loaded() then return end
	if Humanoid() then return end
	if Dead() then return end
	Value = tonumber(Value)
	if noValue(Value) then return end
	Value = math.floor(Value + .5)
	Client.Character.Humanoid.JumpPower = tonumber(Value)
	if Client.PlayerGui.HitEqualsYouDie:FindFirstChild("JumpLimiter") then
		MessageBox.Title.Text = "Disable Fall Damage?"
		MessageBox.Message.Text = "Neptunium's suggestion is for you to disable fall damage, so when jumping to hight you don't hit the ground too hard"
		MessageBox.Visible = true
		Buttons.YesNo.Visible = true
	end
	success(string.format("Set your jumppower to (%s)",tostring(Value)))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set your jumppower to (%s)",tostring(Value)),"Blue")
end
},

["nofall"] = {
Callback = function() 
	if Client.PlayerGui.HitEqualsYouDie:FindFirstChild("JumpLimiter") then
		Client.PlayerGui.HitEqualsYouDie:FindFirstChild("JumpLimiter").Parent = nil
		success("Disabled fall damage")
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Disabled fall damage"),"Blue")
		return
	end
	if not Client.PlayerGui.HitEqualsYouDie:FindFirstChild("JumpLimiter") then
		invalid("Fall damage is already disabled")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Fall damage is already disabled"),"Red")
	end
end
},

["infstam"] = {
Callback = function()
	if LoopStaminaEnabled then
		invalid("Infinite Stamina Is Enabled Already") 
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Infinite Stamina Is Enabled Already"),"Red")
		return
	end
	LoopStaminaEnabled = true
	success("Enabled Infinite Stamina")
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Enabled Infinite Stamina"),"Blue")
end   
},

["uninfstam"] = {
