if v.Name == "Ind_Container1" or v.Name == "Ind_Container2" or  v.Name == "Ind_Container3" or v.Name == "Ind_Container4" or v.Name == "Ind_Container5" then
fireServer("ChangeParent",v, Workspace["ARCL_WALLS"])
fireServer("VehichleLightsSet", Workspace["ARCL_WALLS"], Material, Value)
			end
			end	
			wait(2)
for _, v in pairs(Workspace["ARCL_WALLS"]:GetChildren()) do
if v.Name == "Ind_Container1" or v.Name == "Ind_Container2" or  v.Name == "Ind_Container3" or v.Name == "Ind_Container4" or v.Name == "Ind_Container5" then
fireServer("ChangeParent",v, Workspace["Anchored Objects"]["Towns/Cities"]["Vernal"])
fireServer("VehichleLightsSet",Workspace["Anchored Objects"]["Towns/Cities"]["Vernal"].Lights, "Plastic", 1)
	end
	end

			end
	
	success(string.format("Set " .. Workspace.mapname.Value.. "'s Vernal's Material to (%s) and transparency (%s)",Material,Value))
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set " .. Workspace.mapname.Value.. "'s Vernal Material to (%s) and transparency (%s)",Material,Value),"Blue")
end
