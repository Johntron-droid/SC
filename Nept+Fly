["resetrocks"] = {
Callback = function(Mod,Color,Texture)
	if game.PlaceId == 237590761 or game.PlaceId == 302647266 or game.PlaceId == 1228676522 or game.PlaceId == 1228677045 or game.PlaceId == 290815963 or game.PlaceId == 1228677761 then 
		invalid("Coloring Is Disabled On This Map")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Coloring Is Disabled On This Map"),"Red")
		return
	end
	if not workspace["Anchored Objects"] then 
		invalid("Map Is Deleted")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Map Is Deleted"),"Red")
		return
	end
	if game.PlaceId == 237590657 or game.PlaceId == 1228674372 then
	success("Reseting Rocks to orginal state. please wait...")
		  _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Reseting Rocks to orginal state. please wait..."),"Blue") 
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", Workspace["Anchored Objects"]["Trees/Foliage"], "","","")
		Workspace["Anchored Objects"]["Trees/Foliage"]:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(Workspace["Anchored Objects"]["Trees/Foliage"]["Rocks"],Color, Texture, Color,Texture)
		fireServer("ChangeParent", Workspace["Anchored Objects"]["Trees/Foliage"]:FindFirstChild("SecondaryColor"), nil)
		wait()
		for _, v in pairs(Workspace["Anchored Objects"]["Trees/Foliage"].Rocks:GetChildren()) do
			task.spawn(function()
		if v.Name == "Rock1" or v.Name == "Rock2" or v.Name == "Rock3" then
			Workspace.Remote.AddClothing:FireServer("SecondaryColor", v, "","","")
			v:WaitForChild("SecondaryColor")
			Workspace.Remote.ColorGun:FireServer(v, 36, 12, 36,12)
			end
		end)
	end
		wait(.1)
		for _, v in pairs(Workspace["Anchored Objects"]:GetDescendants()) do
			if v.Name == "SecondaryColor" then
				fireServer("ChangeParent", v, nil)
			end
		end
	end   
	success("Successfully Reset " .. Workspace.mapname.Value.. "'s Rocks")
	_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Successfully Reset " .. Workspace.mapname.Value.. "'s Rocks"),"Blue")
