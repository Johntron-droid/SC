		fireServer("ChangeParent", Workspace["Anchored Objects"]["Trees/Foliage"]:FindFirstChild("SecondaryColor"), nil)
		wait()
		for _, v in pairs(Workspace["Anchored Objects"]["Trees/Foliage"].Rocks:GetChildren()) do
