["novest"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		local VestObject
		for VestName in pairs(VestList) do
			if Player.Character:FindFirstChild(VestName) then
				VestObject = Player.Character[VestName]
			end    
		end
		if not VestObject then 
			invalid(string.format("%s's Vest Is Already Removed",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Vest Is Already Removed",Player.Name),"Red")
			return
		end    
		for VestName in pairs(VestList) do
			if Player:FindFirstChild(VestName) then
				local OldVestObject = Player[VestName]
				fireServer("ChangeParent",OldVestObject, nil)
			end    
		end
		fireServer("ChangeParent",VestObject, Player)
		success(string.format("Removed (%s's) Vest",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Removed (%s) Vest",Player.Name),"Blue")
	end  
end
},

["addvest"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		local VestObject
		for VestName in pairs(VestList) do
			if Player:FindFirstChild(VestName) then
				VestObject = Player[VestName]
			end    
		end
		if not VestObject then 
			invalid(string.format("(%s) Has A Vest Already",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Has A Vest Already",Player.Name),"Red")
			return
		end    
		fireServer("ChangeParent",VestObject, Player.Character)
		success(string.format("Gave (%s's) Vest Back",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Gave (%s) Their Vest Back",Player.Name),"Blue")
	end  
end
},

["noface"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		if not Player.Character.Head:FindFirstChild("face") then 
			invalid(string.format("(%s) Face Is Already Removed",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Face Is Already Removed",Player.Name),"Red")
			return
		end    
		for _, Child in pairs(Player:GetChildren()) do
			if Child.Name == "face" then
				fireServer("ChangeParent",Child, nil)
			end    
		end    
		fireServer("ChangeParent",Player.Character.Head.face, Player)
		success(string.format("Removed (%s) Face",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Removed (%s) Face",Player.Name),"Blue")
	end  
end
},

["addface"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		if Player.Character.Head:FindFirstChild("face") then 
			invalid(string.format("(%s) Has A Face Already",Player.Name))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Has A Face Already",Player.Name),"Red")
			return
		end    
		fireServer("ChangeParent",Player.face, Player.Character.Head)
		success(string.format("Gave (%s's) Face Back",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Gave (%s) Face Back",Player.Name),"Blue")
	end  
end
},

["vest"] = {
Callback = function(Mod,Player, Color, Texture)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if not Color or tonumber(Color) == nil then
			invalid("Missing Or Invalid Color Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Color Input"),"Red")
			return
		end   
		if not Texture or tonumber(Texture) == nil then
			invalid("Missing Or Invalid Texture Input")
			 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Missing Or Invalid Texture Input"),"Red")
			return
		end   
		Color = tonumber(Color)
		Color = math.clamp(Color, 1, 65)
		Color = math.floor(Color + .5)
		Texture = tonumber(Texture)
		Texture = math.clamp(Texture, 1, 12)
		Texture =  math.floor(Texture + .5) 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		local VestObject
		for VestName in pairs(VestList) do
			if Player.Character:FindFirstChild(VestName) then
				VestObject = Player.Character[VestName]
			end    
		end
		if not VestObject then
			invalid(Player.Name .. " Does Not Have A Vest")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Does Not Have A Vest",Player.Name),"Red")
			return
		end    
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", VestObject, "","","")
		VestObject:WaitForChild("SecondaryColor")
		Workspace.Remote.ColorGun:FireServer(VestObject, Color, Texture, Color, Texture)
		wait()
		fireServer("ChangeParent",VestObject:WaitForChild("SecondaryColor"), nil)
		success(string.format("Colored/Textured (%s's) Vest | Color Id (%s) | Texture Id (%s)",Player.Name,Color,Texture))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Colored/Textured (%s) Vest | Color Id (%s) | Texture Id (%s)",Player.Name,Color,Texture),"Blue")
	end    
end
},

["vestmat"] = {
Callback = function(Mod,Player,Material,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Material = GetMaterials(Material)
		if not Material then
			invalid("Missing Or Invalid Material Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Material Input"),"Red")
			return
		end   
		Value = tonumber(Value)
		if noValue(Value) then return end
		Value = math.clamp(Value, 0, 1)
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		local VestObject
		for VestName in pairs(VestList) do
			if Player.Character:FindFirstChild(VestName) then
				VestObject = Player.Character[VestName]
			end    
		end
		if not VestObject then
			invalid(Player.Name .. " Does Not Have A Vest")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Does Not Have A Vest",Player.Name),"Red")
			return
		end    
		fireServer("ChangeParent",VestObject:FindFirstChild("thisisarmor"),nil)
		fireServer("ChangeParent",VestObject:FindFirstChild("thisisarmor"),Player)
		Player:WaitForChild("thisisarmor")
		fireServer("VehichleLightsSet",VestObject,Material,Value)
		fireServer("ChangeParent",Player:FindFirstChild("thisisarmor"),VestObject)
		success(string.format("[✓][Neptunium]: Set (%s) Vest Material To (%s) Transparency (%s)",Player.Name,Material,Value))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Vest Material To (%s) Transparency (%s)",Player.Name,Material,Value),"Blue")
	end    
end
},

["bag"] = {
Callback = function(Mod,Player, Color, Texture)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if not Color or tonumber(Color) == nil then
			invalid("Missing Or Invalid Color Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Color Input"),"Red")
			return
		end   
		if not Texture or tonumber(Texture) == nil then
			invalid("Missing Or Invalid Texture Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Texture Input"),"Red")
			return
		end   
		Color = tonumber(Color)
		Color = math.clamp(Color, 1, 65)
		Color = math.floor(Color + .5)
		Texture = tonumber(Texture)
		Texture = math.clamp(Texture, 1, 12)
		Texture =  math.floor(Texture + .5) 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		local BackPackObject
		for BackpackName in pairs(BackpackList) do
			if Player.Character:FindFirstChild(BackpackName) then
				BackPackObject = Player.Character[BackpackName]
			end    
		end
		if not BackPackObject then
			invalid(Player.Name .. " Does Not Have A Backpack")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Does Not Have A Backpack",Player.Name),"Red")
			return
		end    
		Workspace.Remote.AddClothing:FireServer("SecondaryColor", BackPackObject, "","","")
		BackPackObject:WaitForChild("SecondaryColor")
		workspace.Remote.ColorGun:FireServer(BackPackObject, Color, Texture, Color, Texture)
		wait()
		fireServer("ChangeParent",BackPackObject:WaitForChild("SecondaryColor"), nil)
		success(string.format("Colored/Textured (%s) Bag | Color Id (%s) | Texture Id (%s)",Player.Name,Color,Texture))
		_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Colored/Textured (%s) Bag | Color Id (%s) | Texture Id (%s)",Player.Name,Color,Texture),"Blue")
	end  
end 
},

["bagmat"] = {
Callback = function(Mod,Player,Material,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Material = GetMaterials(Material)
		if not Material then
			invalid("Missing Or Invalid Material Input")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Material Input"),"Red")
			return
		end   
		Value = tonumber(Value)
		if noValue(Value) then return end
		Value = math.clamp(Value, 0, 1)
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		local BackPackObject
		for BackpackName in pairs(BackpackList) do
			if Player.Character:FindFirstChild(BackpackName) then
				BackPackObject = Player.Character[BackpackName]
			end    
		end
		if not BackPackObject then
			invalid(Player.Name .. " Does Not Have A Backpack")
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Does Not Have A Backpack",Player.Name),"Red")
			return
		end 
		fireServer("ChangeParent",Player:FindFirstChild("thisisbackpack"),nil)
		fireServer("ChangeParent",Player:FindFirstChild("WeldScript"),nil)
		fireServer("ChangeParent",BackPackObject:FindFirstChild("thisisbackpack"),Player)
		fireServer("ChangeParent",BackPackObject:FindFirstChild("WeldScript"),Player)
		Player:WaitForChild("thisisbackpack")
		Player:WaitForChild("WeldScript")
		fireServer("VehichleLightsSet",BackPackObject, Material, Value)
		fireServer("ChangeParent",Player:FindFirstChild("thisisbackpack"),BackPackObject)
		fireServer("ChangeParent",Player:FindFirstChild("WeldScript"),BackPackObject)

		success(string.format("Set Material Of (%s's) Backpack",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Backpack Material To (%s) Transparency (%s)",Player.Name,Material,Value),"Blue")
	end    
end
},

["invis"] = {
Callback = function(Mod,Player) 
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notLoaded(Player.Name) then return end
		if notSpawned(Player.Name) then return end
		fireServer("VehichleLightsSet",Player.Character, "Plastic", 1)
		success(string.format("Turned (%s) Invisible",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Turned (%s) Invisible",Player.Name),"Blue")
	end
