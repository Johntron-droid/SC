function deadPlayer(Player)
if not Players[Player].Character:FindFirstChild("Humanoid") or Players[Player].Character:FindFirstChild("Humanoid").Health == 0 then
invalid("(" ..Player .. ") Is Dead")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) Is Dead",Player),"Red")
return true
end 
end

function Loaded()
if not Workspace:FindFirstChild(Client.Name) then 
invalid("Your Either Not Loaded In Or Punished")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Your Either Not Loaded In Or Punished"),"Red")
return true
end
end 

function Spawned()
if not Client.Character.IsSpawned.Value == true then
invalid("Your Not Spawned In Yet")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Your Not Spawned In Yet"),"Red")
return true
end
end

function Humanoid()
if not Client.Character:FindFirstChild("Humanoid") then
invalid("Your Humanoid Is Missing Or Is Removed")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Your Humanoid Is Missing Or Is Removed"),"Red")
return true
end
end

function Dead()
if not Client.Character:FindFirstChild("Humanoid") or Client.Character:FindFirstChild("Humanoid").Health == 0 then
invalid("Your Health Is At 0")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Your Health Is At 0"),"Red")
return true
end
end

function noPlayer(Player)
if not Player then
invalid("Missing Or Invalid Player Input")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: Missing Or Invalid Player Input"),"Red")
return true
end   
end

