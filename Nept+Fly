["kick"] = {
Callback = function(Mod,Player,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		local Reason = tostring(Value)
		if noClient(Player.Name) then return end
		wait(0.25)
		fireServer("ChangeParent", Player, nil)
		 if not Value then Reason = "None" end
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Kicked (%s) | Reason : (%s)",Player.Name,Reason),"Blue")
		 success(string.format("Kicked (%s) | Reason : (%s)",Player.Name,Reason))
		 --game.Workspace.Remote.Chat:FireServer("Global", "[Neptunium]: Kicked ("..Player.Name .. ") | Reason : ("..Reason..")")
	end   
end
