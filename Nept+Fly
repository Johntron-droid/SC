spawn(function() --Source for Script9
local script = Script9
local bar =  script.Parent.Parent.Bar
local gui = script.Parent.Parent.Parent.Parent.Parent.ScreenGui
local main = script.Parent.Parent.Parent.Parent.Load
local side =  script.Parent.Parent.Parent.side
local barhold = script.Parent.Parent.Parent.BarHold
local nept = script.Parent.Parent.Parent.Nept
local loader = script.Parent.Parent.Parent.Loader
local under = script.Parent.Parent.Parent.under
local check =  script.Parent.Parent.Parent.Check
local t =  script.Parent.Parent.Parent.Text
local divide =  script.Parent.Parent.Parent.Divide
local divide2 =  script.Parent.Parent.Parent.Divide2
local v = script.Parent.Parent.Parent.version
local v5 = script.Parent.Parent.Parent.v5
local percent =  script.Parent.Parent.Parent.Percent


wait(0)
main:TweenSize(UDim2.new(0,269,0,84), Enum.EasingDirection.In, Enum.EasingStyle.Sine, 0.3, false)
wait(0.2)
barhold.Visible = true
under.Visible =  true
barhold:TweenSize(UDim2.new(0,254,0,1), Enum.EasingDirection.In, Enum.EasingStyle.Sine, 0.2, false)
under:TweenSize(UDim2.new(0,225,0,1), Enum.EasingDirection.In, Enum.EasingStyle.Sine, 0.3, false)
wait(0.2)
nept.Visible = true
loader.Visible = true
divide.Visible = true
v.Visible = true
v5.Visible = true
divide2.Visible = true
bar.Visible = true
t.Visible = true
check.Visible = true
wait(0.2)
bar:TweenSize(UDim2.new(0,254,0,1), Enum.EasingDirection.In, Enum.EasingStyle.Sine, 3.1, false)
wait(3.6)
main:TweenSize(UDim2.new(0,1,0,84), Enum.EasingDirection.In, Enum.EasingStyle.Sine, 0.2, false)
wait()
nept.Visible = false
loader.Visible = false
divide.Visible = false
v.Visible = false
v5.Visible = false
divide2.Visible = false
bar.Visible = false
t.Visible = false
check.Visible = false
barhold.Visible = false
under.Visible =  false
percent.Visible = false
wait(0.2)
gui:Destroy()
end)
