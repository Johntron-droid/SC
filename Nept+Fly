["banlist"] = {
Callback = function()
	 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Fetching Bans..."),"Blue")
	success("Fetching Bans...")
	wait(0.5)
	local count = 0
	for i,v in pairs(banData) do
		count = count + 1
		local playerName
		local userId = tonumber(i)
		pcall(function()
			playerName = Players:GetNameFromUserIdAsync(userId)
		end)
		info(string.format("(%s) is currently banned",tostring(playerName)))
	end   
	wait()
		if count >= 1 then
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Printed Bans In Console | Amount : (%s)", count),"Blue")
	end
	if count == 0 then
		invalid("No one is banned")
		 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: No One Is Banned"),"Red")
	end
end
},

["bancount"] = {
