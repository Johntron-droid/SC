local function AdaptUI(Scroll, Layout)
Scroll.CanvasSize = UDim2.new(0,0,0,Layout.AbsoluteContentSize.Y+10)
end

function success(Message)
local SuccessMessage = Success:Clone()
SuccessMessage.SuccessReply.Text = Message
SuccessMessage.Parent = Commands
SuccessMessage.Visible = true   
end

function player(Message)
local PlayerMessage = Player:Clone()
PlayerMessage.PlayerReply.Text = Message
PlayerMessage.Parent = Commands
PlayerMessage.Visible = true   
end

function invalid(Message)
local ErrorMessage = Error:Clone()
ErrorMessage.ErrorReply.Text = Message
ErrorMessage.Parent = Commands
ErrorMessage.Visible = true   
end

function warn(Message)
local WarnMessage = Warn:Clone()
WarnMessage.WarnReply.Text = Message
WarnMessage.Parent = Commands
WarnMessage.Visible = true   
end

function info(Message)
local InfoMessage = Info:Clone()
InfoMessage.InfoReply.Text = Message
InfoMessage.Parent = Commands
InfoMessage.Visible = true   
end



local HttpService = game:GetService("HttpService")
local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local TestService = game:GetService("TestService")
local CoreGui = game:GetService("CoreGui")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService = game:GetService("UserInputService")
local Client = Players.LocalPlayer

local function isfile(path)
return (pcall(readfile, path))
end

if not isfile("Neptunium_Bans.txt") then
writefile("Neptunium_Bans.txt", HttpService:JSONEncode({}))
end

if not isfile("Neptunium_Kits.txt") then
writefile("Neptunium_Kits.txt", HttpService:JSONEncode({}))
end

if not isfile("Neptunium_Extra.txt") then
writefile("Neptunium_Extra.txt",HttpService:JSONEncode({}))
writefile("Neptunium_Extra.txt",HttpService:JSONEncode("~"))
end

local mods = {}
local Seperator = [[ ]]
local Mouse = Client:GetMouse()
local banData = HttpService:JSONDecode(readfile("Neptunium_Bans.txt"))
local kits = HttpService:JSONDecode(readfile("Neptunium_Kits.txt"))
Prefix = HttpService:JSONDecode(readfile("Neptunium_Extra.txt"))

--[[
MessageBox.Title.Text = "Neptunium v.n5"
MessageBox.Message.Text = "Welcome, " .. Client.Name .. "!\n Map Name : " .. Workspace.mapname.Value .. "\nChathook Prefix : " .. "(\"" .. Prefix .. "\")".."\n Execution Time : "
Buttons.OkButton.Visible = true 
--]]

info("Neptunium v.n5 : Console Loaded | Dev Build")
info(game:GetService("Players").LocalPlayer.PlayerGui.Sidebar.Bin.ServerID.Text.. " ["..Workspace.mapname.Value.."]")
success(string.format("Chathook : Connected | Prefix : [%s]",Prefix))
player("Type \"".."cmds".. "\" For A List Of Commands")
game:GetService("CoreGui").Neptunium.Console.Text.Commands.Player.PlayerReply.TextColor3 = Color3.fromRGB(150, 150, 150)
game:GetService("CoreGui").Neptunium.Console.Text.Commands.Success.SuccessReply.TextColor3 = Color3.fromRGB(136, 219, 125)

local _G_env = (pcall(getsenv, game.Players.LocalPlayer.Backpack.GlobalFunctions) and getsenv(game.Players.LocalPlayer.Backpack.GlobalFunctions)) or (pcall(getrenv) and   getrenv) or nil
local MetaCall = getrawmetatable(_G_env.shared)
local RemoteCall = debug.getupvalues(debug.getupvalues(MetaCall.__index)[3])
Serial = RemoteCall[6]
GetKey = RemoteCall[7]
function fireServer(Re, ...)
workspace.Remote[tostring(Re)]:FireServer(Serial({...}, GetKey()))
end

_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Welcome, %s",game.Players.LocalPlayer.Name),"Blue")
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Console Loaded | Version v.n5"),"Blue")
_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Execution Time : (%.3f ms)",(tick() - Time)),"Blue")

function AntiKick()
repeat
wait()
until Client.Character ~= nil and Client.Character:FindFirstChild("HumanoidRootPart")
Client.Character.HumanoidRootPart.ChildAdded:connect(function(Child)
if  tostring(Child) == "IsBuildingMaterial" then
	Workspace.Remote.Detonate:FireServer(Child)
	wait()
	fireServer("ChangeParent",Child, nil)
end
end)
end

Client.CharacterAdded:connect(AntiKick)
spawn(function()
AntiKick()
end)

function wallbang()
return  _G_env._G.IgnoreParts
end

local checkmods = function(player)
for _, v in next, admins do
if v:lower() == player:lower() then
	return true
end
end
end

Players.PlayerRemoving:Connect(function(Player)
if table.find(mods, Player.Name) then
table.remove(mods,table.find(mods, Player.Name)) 
end
end)

local function KickPlayer(Player, yT)
yT = yT or .25
Workspace.Remote.AddClothing:FireServer("PermanentBan", Player, "", "", "")
wait(yT)
fireServer("ChangeParent", Player, nil)
end

ServerLock = false
function ServerLockF()
ServerLock = not ServerLock
return ServerLock
end
Players.PlayerAdded:Connect(function(Player)
if ServerLock == true then
_G_env["_G"].SendMessage(string.format("[卍][ServerLock]: Enabled | Auto Kicked (%s)",Player.Name),"Yellow")
warn(string.format("[卍][ServerLock]: Enabled | Auto Kicked (%s)",Player.Name))
KickPlayer(Player, .25)
end
end)

spawn(function()
for i in pairs(banData) do
local userId = tonumber(i)
local playerName
pcall(function()
	playerName = Players:GetNameFromUserIdAsync(userId)
end)
if playerName and Players:FindFirstChild(playerName) then
	_G_env["_G"].SendMessage(string.format("[卍][Neptunium]: (%s) Is On The Banlist | Auto Kicked",playerName),"Yellow")
	warn(string.format("[卍][Banlist]: (%s) is On Banlist | Auto Kicked",playerName))
	KickPlayer(Players[playerName], .25)
end
end
end)

Players.PlayerAdded:Connect(function(Player)
if banData[tostring(Player.UserId)] then
 _G_env["_G"].SendMessage(string.format("[卍][Neptunium]: Banlist Player (%s) Tried Joining | Auto Kicked",Player.Name),"Yellow")
 warn(string.format("[卍][Banlist]: (%s) is On Banlist | Auto Kicked",Player.Name))
 KickPlayer(Player, .25)
end
end)

local function changeShirt (player, id)
if not Players[player].Character:FindFirstChild("GameShirt") then
return
end

local shirt = Players[player].playerstats.character.shirt
id = id or 0
fireServer("ChangeValue", shirt.ObjectID.Shirt, id)

Workspace.Remote.AddClothing:FireServer("anything lol", shirt,"","","")
wait()
fireServer("ChangeParent",shirt:WaitForChild("anything lol"),nil)
end

local function changePants(player, id)
if not Players[player].Character:FindFirstChild("GameShirt") then
return
end

local pants = Players[player].playerstats.character.pants
id = id or 0
fireServer("ChangeValue", pants.ObjectID.Pants, id)

Workspace.Remote.AddClothing:FireServer("anything lol", pants,"","","")
wait()
fireServer("ChangeParent",pants:WaitForChild("anything lol"),nil)
end

checkpermission = function(player)
for _, v in next, mods do
if v:lower() == player:lower() then
	return true
end
end
end

local function setParent(instance, parent)
fireServer("ChangeParent", instance, parent)
repeat wait() until instance.Parent == parent
end

local function destroy(instance)
fireServer("ChangeParent", instance)
end

local cloneBait = {
"G18Ammo",
"VehicleJack"
}
local function clone(instance, parent, amount)
amount = amount or 1

local bait
local ogParent = instance.Parent
local cloneEvent
local cloneYield = true
local objects = {}
local objectIterator = 0
local data = {}

for i, item in pairs(cloneBait) do
local found = Lighting.LootDrops:FindFirstChild(item)
if found then
	setParent(found, Lighting.Materials)
	setParent(instance, found)

	bait = found
	break
end
end

cloneEvent = Workspace.ChildAdded:Connect(function(child)
if bait == nil then return end
if child.Name == bait.Name then
	repeat wait() until child:FindFirstChildOfClass("Model")
	local clone = child:FindFirstChild(instance.Name)
	if clone then
		setParent(clone, parent)
		destroy(child)

		objectIterator = objectIterator + 1
		objects[objectIterator] = clone

		if objectIterator >= amount then
			setParent(instance, ogParent)
			setParent(bait, Lighting.LootDrops)

			cloneEvent:Disconnect()
			cloneYield = false
		end
	end
end
end)
for i = 1, amount do
Workspace.Remote.PlaceMaterial:FireServer(bait.Name, Vector3.new(9e9,9e9,9e9), false, false)
end 

repeat wait() until not cloneYield

data.objects = objects
function data.forEach(callback)
for i,v in pairs(objects) do
	callback(v)
end
end
return data
end



function GetPart(Model)
local Part = nil
for _, v in pairs(Model:GetDescendants()) do
if v:IsA("BasePart") and (Part == nil or tostring(v) == "Head" or tostring(v) == "Driver") then
	Part = v
end
end
return Part
end

local Amount = 0
MaxAmount = 1000
TimeToWait = 1.5
function Limiter()
Amount = Amount + 1
if Amount >= MaxAmount then
wait(TimeToWait)
Amount = 0
end
end

VehiclesTable = {}
for _, v in pairs(Workspace.Vehicles:GetChildren()) do
if VehiclesTable[tostring(v)] == nil then
VehiclesTable[tostring(v)] = true
end
end
Workspace.Vehicles.ChildAdded:connect(function(Child)
wait()
if VehiclesTable[tostring(Child)] == nil then
VehiclesTable[tostring(Child)] = true
end
end)
Workspace.ChildAdded:connect(function(Child)
wait()
if VehiclesTable[tostring(Child)] ~= nil then
fireServer("ChangeParent", Child, Workspace.Vehicles)
end
end)

SpawnedItems = {}
function Clone(Player, Item, Parent, Offset, Spawn)
if Player == nil or Item == nil or GetItemFromString(Item, Parent) == nil then
return
end
Item = GetItemFromString(Item, Parent)
if Spawn == nil then
Limiter()
end
if SpawnedItems[Item] == nil then
SpawnedItems[Item] = {true}
else
table.insert(SpawnedItems[Item], true)
end
if Item.Parent ~=  Lighting.Materials then
fireServer("ChangeParent", Parent:WaitForChild(tostring(Item)), Lighting.Materials)
end
local ItemWait = Lighting.Materials:WaitForChild(tostring(Item))
if ItemWait.PrimaryPart == nil then
ItemWait.PrimaryPart = GetPart(ItemWait)
end
pcall(function() Workspace.Remote.PlaceMaterial:FireServer(Lighting.Materials:WaitForChild(tostring(Item)).Name,Player.Character.Torso.Position-ItemWait.PrimaryPart.Position-Offset) end)
spawn(function()
wait(2)
table.remove(SpawnedItems[Item], 1)
if #SpawnedItems[Item] < 1 then
	fireServer("ChangeParent", Lighting.Materials:WaitForChild(tostring(Item)), Parent)
end
end)
end

