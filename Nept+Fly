local Connections = {}
local function closeService()
for _, event in pairs(Connections) do
event:Disconnect()
end
CoreGui['Neptunium']:Destroy()
script:Destroy()
end

local function GetPlayer(str)
if str == nil or str == "" then return end
for _, Player in pairs(Players:GetPlayers()) do 
if Player.Name:lower() == str:lower() then
	return Player.Name
end
end
for _, Player in pairs(Players:GetPlayers()) do
if Player.Name:lower():sub(1, #str) == str:lower() then
	return Player.Name
end
end
return 
end

