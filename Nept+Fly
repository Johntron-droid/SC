-- weapon mods 

["ammo"] = {
Callback = function(Mod,Player,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Value = tonumber(Value)
		if noValue(Value) then return end
		Value = math.clamp(Value, 1, 99999999999999) 
		Value = math.floor(Value + .5)
		if notSpawned(Player.Name) then return end
		for _, v in next, Player.playerstats.slots:GetChildren() do
			if v:FindFirstChild("ObjectID") then
				if v:FindFirstChild("ObjectID"):FindFirstChild("Clip") then 
					fireServer("ChangeValue", v.ObjectID.Clip.MaxClip, Value)
					fireServer("ChangeValue", v.ObjectID.Clip,  _G_env._G.Obfuscate(Value))
				end
			end
		end
		success(string.format("Set (%s's) Ammo To (%s)",Player.Name,tostring(Value)))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Ammo To (%s)",Player.Name,tostring(Value)),"Blue")
	end  
end
},

["infammo"] = {
Callback = function(Mod,Player)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		if notSpawned(Player.Name) then return end
		for _, v in next, Player.playerstats.slots:GetChildren() do
			if v:FindFirstChild("ObjectID") then
				if v:FindFirstChild("ObjectID"):FindFirstChild("Clip") then 
					fireServer("ChangeValue", v.ObjectID.Clip.MaxClip, 99999999999999)
					fireServer("ChangeValue", v.ObjectID.Clip,  _G_env._G.Obfuscate(99999999999999))
				end
			end
		end
		success(string.format("Set (%s's) Ammo To 99999999999999",Player.Name))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Ammo To 99999999999999",Player.Name),"Blue")
	end  
end
},

["spread"] = {
Callback = function(Mod,Player,Weapon,Value)
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Weapon = GetWeaponName(Weapon)
		if noWeapon(Weapon) then return end
		Value = tonumber(Value)
		if noValue(Value) then return end
		Value = math.clamp(Value, 1, 99999999999999) 
		Value = math.floor(Value + .5)
		if notSpawned(Player.Name) then return end
		local Can = false
		for _, WeaponObj in pairs(Player.Backpack:GetChildren()) do
			if WeaponObj.Name == Weapon then
				Can = true
			end
		end
		if not Can then
			invalid(string.format("(%s) does not have a (%s)",Player.Name,Weapon))
			 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: (%s) does not have a (%s)",Player.Name,Weapon),"Red")
			return 
		end
		fireServer("ChangeParent",Player.Backpack[Weapon].Shooter,Player)
		wait()
		fireServer("ChangeValue", Player.Backpack[Weapon].Stats.Accuracy,  _G_env._G.Obfuscate(Value))
		wait(.1)
		fireServer("ChangeParent",Player.Shooter,Player.Backpack[Weapon])
		success(string.format("Set (%s's) (%s) Spread To (%s)",Player.Name,Weapon,tostring(Value)))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s's) (%s) Spread To (%s)",Player.Name,Weapon,tostring(Value)),"Blue")
	end 
end
},

