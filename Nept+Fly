function noClient(Player)
if Player == Client.Name then 
invalid("This Command Does Not Apply On Client")
 _G_env["_G"].SendMessage(string.format("[!][Neptunium]: This Command Does Not Apply On Client"),"Red")
return true
end
end

function SkyboxRenderMode()
if Client.PlayerGui:FindFirstChild("SkyboxRenderMode") then
local path = Client.PlayerGui.SkyboxRenderMode
path.Parent = game.StarterGui
end
end

local function returnPlayers(Mod,phrase)
local players = {}

if phrase == nil then 
phrase = GetPlayer(phrase)
if noPlayer(phrase) then return players end
end

if phrase:lower() == "me" then
table.insert(players, Players[Mod])
elseif phrase:lower() == "others" then
for _, v in pairs(Players:GetChildren()) do
	if v ~= Players[Mod] then 
		table.insert(players, v)
	end
end
elseif phrase:lower() == "all" then 
for _, v in pairs(Players:GetChildren()) do
	table.insert(players, v)
end
else
phrase = GetPlayer(phrase)
if noPlayer(phrase) then return players end
table.insert(players,Players[phrase])
end
return players
end

function CheckNumber(Numb)
return tonumber(string.sub(tostring(Numb), 1, 1))
end
--- rocket
Mats = Lighting.Materials
local RM = workspace.Remote
getgenv().DoWeld = function(x,y)
local CJ = CFrame.new(x.Position)
local C0 = x.CFrame:inverse()*CJ
local C1 = y.CFrame:inverse()*CJ
MakeWeld(x,y, C0, C1)
end
function MakeWeld(Part0, Part1, C0, C1)
if C0 == nil then
C0 = CFrame.new()
end
if C1 == nil then
C1 = CFrame.new()
end
RM.CreateWeld:FireServer(Part0, Part1, C0, C1)
local W = Instance.new("Weld", Part0)
W.Part0 = Part0
W.Part1 = Part1
W.C0 = C0
W.C1 = C1
end

function PlaceFlare(Player)
if Player == nil or Player.Character == nil or not Player.Character:FindFirstChild("Head") then
return
end
local Head = Player.Character.Head
local P = Head.Position
local Flare = Mats.RoadFlareLit
RM.PlaceMaterial:fireServer("RoadFlareLit", P-Flare.Head.Position)
while wait() do
for i, v in pairs(game.Workspace:GetChildren()) do
	if v.Name == "RoadFlareLit" and (v.Head.Position-P).Magnitude < 2 then
		MakeWeld(v.Head, Head, CFrame.new(0, 2, 0))
		return v
	end
end
end
end

