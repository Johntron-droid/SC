--victini detect
ExploitersList = {}
function checkForVictini(Tab)
if Tab["RequestType"] == "Script" then
local PScript = Tab["Message"]
local PlrSent = Tab["Player"]
if PScript ~= nil and type(PScript) == "string" and game.Players:FindFirstChild(tostring(PlrSent)) then
	function Callback(answer)
		if answer == "Kick" then
			KickPlayer(PlrSent, .25)
			_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Kicked (%s)",PlrSent.Name),"Blue")
			success(string.format("Kicked (%s)",PlrSent.Name))
		elseif answer == "Kill" then
			if deadPlayer(PlrSent.Name) then return end
			fireServer("ChangeParent", PlrSent.Character:FindFirstChild("Head"), nil)
			_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Killed (%s)",PlrSent.Name),"Blue")
			success(string.format("Killed (%s)",PlrSent.Name))
		end
	end
	Bindable = Instance.new("BindableFunction")
	Bindable.OnInvoke = Callback
	game:GetService("StarterGui"):SetCore("SendNotification",{
		Title = "    [Neptunium]", 
		Text = "(".. PlrSent.Name .. ") Executed VictiniV2!", 
		Icon = "https://www.roblox.com/asset-thumbnail/image?assetId=376938026&width=420&height=420&format=png";
		Button1 = "Kill";
		Button2 = "Kick",
		Duration = 8,
		Callback = Bindable,
	})
	print("Player ran VictiniV2: " .. tostring(PlrSent.Name))
	warn(string.format("[!][Detection]: Little Jew (%s) Executed VictiniV2!",PlrSent.Name))
	_G_env["_G"].SendMessage(string.format("[!][D][Neptunium]: Little Fat Jew (%s) Executed VictiniV2!",PlrSent.Name),"Yellow")
	ExploitersList[tostring(PlrSent)] = PScript
end
end
end
game.Workspace.Remote.ChangeParentLocal.OnClientEvent:connect(function(Tab)
if type(Tab) == "table" then
checkForVictini(Tab)
end
end)


