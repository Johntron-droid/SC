Client.PlayerGui.HitEqualsYouDie:FindFirstChild("JumpLimiter").Parent = nil
MessageBox.Visible = false
Buttons.YesNo.Visible = false
end)
Buttons.YesNo.No.MouseButton1Down:Connect(function()
MessageBox.Visible = false
Buttons.YesNo.Visible = false
end)

