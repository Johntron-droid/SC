["invgroup"] = {
	Callback = function(Mod,Player)
	   local Playerlist = returnPlayers(Mod,Player)
		 for _, Player in pairs(Playerlist) do 
			if checkgroup(Mod) then return end
			Workspace.Remote["GroupInvite"]:FireServer(Player, getgroup(Mod))
			success(string.format("Sent (%s) an invite to (%s) group", Player.Name, Mod))
			_G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Sent (%s) an invite to (%s) group", Player.Name, Mod),"Blue")
		end
	end
},

["joingroup"] = {
	  Callback = function(Mod,Player)
		local Playerlist = returnPlayers(Mod,Player)
		 for _, Player in pairs(Playerlist) do 
			if checkgroup(Player.Name) then return end
			Workspace.Remote["GroupInvite"]:FireServer(Players[Mod], getgroup(Player.Name))
			success(string.format("Retrieved invite to (%s) group", Player.Name))
			 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Retrieved invite to (%s) group", Player.Name),"Blue")
		end
	end
},



["kickgroup"] = {
	Callback = function(Mod,Player)
		local Playerlist = returnPlayers(Mod,Player)
		 for _, Player in pairs(Playerlist) do 
			if checkgroup(Player.Name) then return end
			Workspace.Remote["GroupKick"]:FireServer(Lighting.Groups[getgroup(Player.Name)], Player)
			success(string.format("Kicked (%s) from his group", Player.Name))
			 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Kicked (%s) from his group",Player.Name),"Blue")
		end
	end
},

["pkills"] = {
Callback = function(Mod,Player,Value) 
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Value = tonumber(Value)
		if noValue(Value) then return end
		fireServer("ChangeValue", Player.playerstats.PlayerKill.Bandit, Value)
		success(string.format("Set (%s) Kills To (%s)",Player.Name, tostring(Value)))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Kills To (%s)",Player.Name, tostring(Value)),"Blue")
	end
end
},

["days"] = {
Callback = function(Mod,Player,Value) 
	local Playerlist = returnPlayers(Mod,Player)
	for _, Player in pairs(Playerlist) do 
		Value = tonumber(Value)
		if noValue(Value) then return end
		fireServer("ChangeValue", Player.playerstats.Days, Value)
		success(string.format("Set (%s) Days To (%s)",Player.Name, tostring(Value)))
		 _G_env["_G"].SendMessage(string.format("[✓][Neptunium]: Set (%s) Days To (%s)",Player.Name, tostring(Value)),"Blue")
	end
end
},

